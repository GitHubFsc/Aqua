div.check-box {
  display: inline-block;
  position: relative;
  width: 15px;
  height: 15px;
  border: 1px solid #c0c0c0;
}

div.check-box-focus {
  position: absolute;
  left: 2px;
  top: 2px;
  background-color: #58b5e0;
  width: 9px;
  height: 9px;
  display: none;
}

div.check-box.disabled div.check-box-focus {
  background-color: rgb(184, 184, 184);
}

div.check-box > input {
  display: none;
}

input:checked + div.check-box-focus {
  display: block;
}

div.check-box + span {
  text-align: left !important;
  min-width: auto !important;
  padding-left: 10px;
}

