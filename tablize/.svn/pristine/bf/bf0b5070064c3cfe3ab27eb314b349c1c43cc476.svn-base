#au_time_picker_date_picker_start > input, #au_time_picker_date_picker_end >input,
#channel_history_au_time_picker_date_picker_start > input, #channel_history_au_time_picker_date_picker_end >input  {
	cursor: default;
}

#au_time_picker_date_picker_start, #channel_history_au_time_picker_date_picker_start {
	top: 30px;
	z-index: 20;
}

#au_time_picker_date_picker_end,#channel_history_au_time_picker_date_picker_end {
	top: 90px;
	z-index: 15;
}

#au_time_picker_shortcut,#channel_history_au_time_picker_shortcut {
	position: absolute;
	left: 15px;
