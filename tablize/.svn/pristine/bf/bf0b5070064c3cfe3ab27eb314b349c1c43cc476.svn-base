	height: 30px;
	line-height: 30px;	
}

#au_time_picker_shortcut_enable,#channel_history_au_time_picker_shortcut_enable {
	vertical-align: middle;
	margin-right: 5px;
}

#au_time_picker_shortcut_input,#channel_history_au_time_picker_shortcut_input {
	width: 80px;
	height: 30px;
	display: inline;
	color: rgb(121,121,121);
	margin-left: 5px;
	margin-right: 5px;	
}

.channel_history_day_label {
	width: 100%;
	height: 15px;
	line-height: 15px;
	text-align: center;
	color: rgb(121,121,121);	
}

#programHistory_chartContent > div.program_history_zoom_one {
	border-color: rgb(18,160,234);
	border-width: 1px 1px 0 1px;
	border-style: solid;
}

#programHistory_chartContent > div.program_history_zoom_others {

}

.program_history_time_label {
	margin: 0;
	padding: 0;
	position: relative;
	text-align: center;
}
