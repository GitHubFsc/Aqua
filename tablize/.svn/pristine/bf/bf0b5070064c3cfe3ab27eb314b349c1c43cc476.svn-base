	top: 150px;
	left: 115px;
}

.au_time_picker_dual #au_time_picker_end_hour, .au_time_picker_dual #channel_history_au_time_picker_end_hour {
	top: 220px;
	left: 15px;
}

.au_time_picker_dual #au_time_picker_end_min, .au_time_picker_dual #channel_history_au_time_picker_end_min {
	top: 220px;
	left: 115px;
}

#au_time_picker_date_picker_start, #au_time_picker_date_picker_end,#channel_history_au_time_picker_date_picker_start, #channel_history_au_time_picker_date_picker_end {
	position: absolute;
	width: 150px;
	height: 30px;
	left: 15px;
}

#au_time_picker_date_picker_start > div, #au_time_picker_date_picker_end >div,
#channel_history_au_time_picker_date_picker_start > div, #channel_history_au_time_picker_date_picker_end >div {
