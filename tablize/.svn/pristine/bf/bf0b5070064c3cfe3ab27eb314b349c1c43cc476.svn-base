.au_analysis {
	position: relative;
	width: 100%;
	height: 100%;
}

.au_analysis table {
	font-size: 14px;
}

.au_analysis p {
	margin: 0;
	padding: 0;
}

.au_analysis_head {
	position: relative;
	width: 100%;
	height: 50px;
	background-color: white;
}

#au_analysis_titletxt {
	position: absolute;
	height: 20px;
	width: 75px;
	left: 55px;
	top: 15px;
}

#au_analysis_clock {
	position: absolute;
	margin: 0;
	padding: 0;
	height: 20px;
	width: 170px;
	line-height: 20px;
	font-family: Arial;
	font-size: 14px;
	color: rgb(150,154,176);
	right: 25px;
	top: 15px;
}

.au_analysis_body {
	position: relative;
	width: 100%;
	height: calc(100% - 51px);
	border-color: rgb(226,226,226);
	border-width: 1px 0 0 0;
	border-style: solid;
	background-color: rgb(242,242,242);
}

#au_analysis_content {
	position: absolute;
	top: 20px;
	left: 25px;
	bottom: 0;
	right: 25px;
}

.au_analysis_content_head {
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	height: 40px;
	background-color: rgb(18,160,234);
	z-index: 10;
}

.au_analysis_content_titletxt {
	position: absolute;
	left: 10px;
	top: 10px;
	height: 20px;
	line-height: 20px;
	font-size: 14px;
	color: white;
	margin: 0;
	padding: 0;
	font-weight: bold;
}

.au_analysis_content_body {
	position: absolute;
	left: 0;
	top: 45px;
	right: 0;
	bottom: 0;
	background-color: white;
	z-index: 1;
	font-size: 14px;
}

#channel_realtime_channel_filter {
	position: absolute;
	right: 80px;
	height: 25px;
	width: 300px;
	top: 8px;
}

#channel_realtime_chart_filter {
	position: absolute;
	right: 9px;
	height: 25px;
	width: 64px;
	top: 8px;
}

#channel_realtime_chart_vertical {
	width: 100%;
	height: 100%;
	background-position: center;
	background-repeat: no-repeat;
	background-image: url(../images/realtimeAnalysis/chart_filter_vertical.png);	
}

#channel_realtime_chart_horizontal {
	width: 100%;
	height: 100%;
	background-position: center;
	background-repeat: no-repeat;
	background-image: url(../images/realtimeAnalysis/chart_filter_horizontal.png);	
}
.au_analysis_channel_picker {
	position: relative;
	width: 655px;
	height: 265px;
	background-color: white;
	border: 1px solid rgb(229,229,229);
}

.au_analysis_channel_picker_switcher {
	position: absolute;
	width: 100%;
	height: 30px;
}

.au_analysis_channel_picker_display {
	position: absolute;
	left: 20px;
	right: 20px;
	top: 35px;
	bottom: 41px;
	border-width: 0 0 1px 0;
	border-color: rgb(222,222,222);
	border-style: solid;
	overflow-x: auto;
}

.au_analysis_channel_picker_cancel_button, .au_analysis_channel_picker_submit_button {
	position: absolute;
	line-height: 30px;
	height: 30px;
	width: 80px;
	color: white;
	border: 1px solid rgb(33,136,190);
	bottom: 5px;
	text-align: center;
	cursor: pointer;
}

.au_analysis_channel_picker_cancel_button {
	background-color: rgb(100,184,228);
	right: 110px;
}

.au_analysis_channel_picker_submit_button {
	background-color: rgb(63,173,231);
	right: 20px;
}

.au_analysis_channel_picker_reset_button {
	position: absolute;
	line-height: 30px;
	height: 30px;
	width: 40px;
	text-decoration: underline;
	color: rgb(121,121,121);
	bottom: 5px;
	right: 200px;
	cursor: pointer;
}

#channel_history_channel_filter {
	position: absolute;
	width: 80px;
	height: 30px;
	right: 70px;
	top: 5px;	
}

#channel_history_time_filter {
	position: absolute;
	width: 60px;
	height: 30px;
	right: 5px;
	top: 5px;
}

.au_analysis_time_picker {
	position: relative;
	width: 480px;
	height: 240px;
	border: 1px solid rgb(203,203,203);
	background-color: rgb(239,239,239);
}

.au_analysis_time_picker_content {
	position: absolute;
	left: 10px;
	right: 10px;
	top: 10px;
	bottom: 41px;
	border-width: 0 0 1px 0;
	border-color: rgb(208,208,208);
	border-style: solid;
	color: rgb(121,121,121);
	z-index: 10;
}

#au_time_picker_calendar,#programHistory_au_time_picker_calendar {
	position: absolute;
	left: 0;
	top: 0;
	bottom: 5px;
	width: 210px;
	font-family: 'Microsoft YaHei';
	font-size: 12px;
}

.au_analysis_time_picker_times_cont {
	position: absolute;
	top: 0;
	right: 0;
	width: 200px;
	bottom: 5px;
}

.au_time_picker_label {
	position: absolute;
	height: 20px;
	line-height: 20px;
	text-align: left;
}

#au_time_picker_start_hour, #au_time_picker_start_min, #au_time_picker_end_hour, #au_time_picker_end_min,
#programHistory_au_time_picker_start_hour, #programHistory_au_time_picker_start_min, #programHistory_au_time_picker_end_hour, #programHistory_au_time_picker_end_min,
#channel_history_au_time_picker_start_hour, #channel_history_au_time_picker_start_min, #channel_history_au_time_picker_end_hour, #channel_history_au_time_picker_end_min{
	position: absolute;
	width: 60px;
	height: 30px;
	background-color: white;
}

#au_time_picker_start_hour,#programHistory_au_time_picker_start_hour,#channel_history_au_time_picker_start_hour {
	top: 24px;
	z-index: 10;
}

#au_time_picker_start_min,#programHistory_au_time_picker_start_min,#channel_history_au_time_picker_start_min {
	top: 24px;
	left: 100px;
	z-index: 10;
}

#au_time_picker_end_hour,#programHistory_au_time_picker_end_hour,#channel_history_au_time_picker_end_hour {
	top: 95px;
	z-index: 5;
}

#au_time_picker_end_min,#programHistory_au_time_picker_end_min, #channel_history_au_time_picker_end_min{
	top: 95px;
	left: 100px;
	z-index: 5;
}

.au_time_picker_split_front, .au_time_picker_split_back {
	position: absolute;
	height: 30px;
	width: 30px;
	line-height: 30px;	
}

.au_time_picker_split_front {
	text-align: center;
}

.au_time_picker_split_back {
	text-align: left;
}

.au_analysis .calendar-year-label, .au_analysis .calendar-month-label {
	font-size: 12px;
}

.au_analysis .styled-selector-dropbox {
	z-index: 10;
	border: 1px solid rgb(203,203,203);
}

.au_analysis .styled-selector-inputbox {
	color: rgb(121,121,121);
	border: 1px solid rgb(203,203,203);
}

.au_analysis_time_picker_times_cont .styled-selector-inputbox, .au_analysis_time_picker_times_cont .styled-selector-dropbox td{
	padding-left: 5px;
}

.au_analysis_time_picker_times_cont .styled-selector-dropbox td:hover {
	background-color: rgb(200,200,200);
}

#channel_history_chart_container {
	position: absolute;
	left: 0;
	top: 0;
	bottom: 0;
	right: 330px;
}

#channel_history_chart_content {
	position: absolute;
	right: 330px;
	bottom: 0;
	top: 0;
	left: 45px;
	z-index: 10;
	overflow-x: auto;
	overflow-y: hidden;
	word-break: keep-all;
	white-space: nowrap;
	text-overflow: ellipsis;
}

#channel_history_chart_content > div {
	min-width: 80px;
}

#channel_history_chart_content > div:nth-child(2n) {
	background-color: rgba(228,246,250, 0.5);
	border-style: none;	
}

#channel_history_list_container {
	position: absolute;
	top: 0;
	bottom: 0;
	right: 0;
	width: 300px;
	overflow-x: hidden;
	overflow-y: auto;
}

.channel_history_content_spliter {
	position: absolute;
	right: 314px;
	width: 2px;
	top: 20px;
	bottom: 20px;
	background-color: rgb(235,235,235);
}

.rating_line_chart_label {
	text-align: center;
	margin: 0;
	padding: 0;
	height: 15px;
	line-height: 15px;
	color: rgb(121,121,121);
}

#channel_history_duration_indicator {
	position: absolute;
	height: 20px;
	line-height: 20px;
	left: 150px;
	top: 10px;
	margin: 0;
	padding: 0;
	color: white;
	font-size: 14px;
}

.channel_history_info_list {
	position: relative;
	width: 100%;
	table-layout: fixed;
	text-align: center;
	color: rgb(121,121,121);
}

.channel_history_info_list td {
	position: relative;
	height: 30px;
	line-height: 30px;
	overflow: hidden;
	word-break: keep-all;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.channel_history_info_list td:nth-child(1){
	text-align: left;
}

.channel_history_info_list_colorRect {
	display: inline-block;
	vertical-align: middle;
	margin-left: 10px;
	margin-right: 5px;
}

#programHistory_durationIndicator {
	position: absolute;
	height: 20px;
	line-height: 20px;
	left: 160px;
	top: 10px;
	margin: 0;
	padding: 0;
	color: white;
	font-size: 14px;	
}

#programHistory_filter {
	position: absolute;
	right: 5px;
	top: 5px;
	height: 30px;
	width: 85px;	
}

#programHistory_chartContainer {
	
}

#programHistory_filter #au_time_picker_start_hour,#programHistory_filter #programHistory_au_time_picker_start_hour {
	top: 79px;
	z-index: 10;
}

#programHistory_filter #programHistory_au_time_picker_start_min,#programHistory_filter #au_time_picker_start_min {
	top: 79px;
	left: 100px;
	z-index: 10;
}

#programHistory_filter #au_time_picker_end_hour,#programHistory_filter #programHistory_au_time_picker_end_hour {
	top: 150px;
	z-index: 5;
}

#programHistory_filter #au_time_picker_end_min,#programHistory_filter #programHistory_au_time_picker_end_min {
	top: 150px;
	left: 100px;
	z-index: 5;
}

#au_time_picker_channels_dropdown,#programHistory_au_time_picker_channels_dropdown {
	position: absolute;
	left: 0;
	width: 160px;
	height: 30px;
	top: 25px;
}

#programHistory_chartContent {
	position: absolute;
	right: 0;
	bottom: 0;
	top: 0;
	left: 45px;
	z-index: 10;
	overflow-x: auto;
	overflow-y: hidden;
	word-break: keep-all;
	white-space: nowrap;
}

#programHistory_chartContent > div:nth-child(2n-1) {
	background-color: rgba(228,246,250, 0.5);
	border-style: none;	
}

.programHistory_dayLabel {
	width: 100%;
	height: 30px;
	line-height: 30px;
	top: 100%;
	position: absolute;
	text-align: center;
	color: rgb(121,121,121);
}

.program_history_time_labels_container {
	width: 100%;
	height: 30px;
	line-height: 30px;
	top: 100%;
	position: absolute;
	color: rgb(121,121,121);
}

.programHistory_dayAvgLabel {
	position: absolute;
	right: 0;
	bottom: 0;
	width: 65px;
	height: 60px;
	line-height: 20px;
	color: rgb(121,121,121);
	text-align: right;	
}

.au_analysis p.programHistory_dayAvgLabel {
	padding-right: 5px;
}

#programHistory_channelAvgLabel {
	position: absolute;
	right: 10px;
	top: 5px;
	line-height: 25px;
	height: 25px;
	color: rgb(121,121,121);
	z-index: 20;
}

#channel_realtime_colors {
	position: absolute;
	right: 0;
	top: 0;
	width: 250px;
	height: 30px;
	line-height: 30px;
	color: rgb(121,121,121);
}

#channel_realtime_colors > p {
	display: inline-block;
	height: 30px;
	line-height: 30px;
	vertical-align: middle;
}

.channel_realtime_color {
	display: inline-block;
	width: 20px;
	height: 20px;
	border-radius: 2px;
	vertical-align: middle;
	margin-left: 15px;
	margin-right: 5px;
}
div.channel_realtime_color:nth-of-type(1){
	background-color: rgb(55,200,242);
}
div.channel_realtime_color:nth-of-type(2){
	background-color: rgb(242,54,107);
}
div.channel_realtime_color:nth-of-type(3) {
	background-color: rgb(32,205,130);
}

#channel_history_tooltip {
	position: absolute;
	width: 20px;
	height: 20px;
}

#channel_history_tooltip_icon {
	position: absolute;
	width: 14px;
	height: 14px;
	border: 1px solid rgb(55,200,242);
	border-radius: 7px;
	background-color: white;
	top: -7px;
	left: -7px;
}

#channel_history_tooltip_icon > div {
	position: absolute;
	width: 8px;
	height: 8px;
	background-color: rgb(55,200,242);
	border-radius: 4px;
	left: 3px;
	top: 3px;
}

#channel_history_tooltip_content {
	position: absolute;
	background-color: rgb(55,200,242);
	border: 1px solid rgb(38,170,208);
	border-radius: 5px;
	color: white;
	left: 7px;
	bottom: 27px;	
}

#channel_history_tooltip_content > p {
	height: 20px;
	line-height: 20px;
	width: 140px;
	overflow: hidden;
	word-break: keep-all;
	white-space: nowrap;
	text-overflow: ellipsis;
	padding-left: 3px;
	padding-right: 3px;
}

.au_time_picker_dual {
	width: 215px;
	height: 390px;
}

.au_time_picker_dual #au_time_picker_start_hour,.au_time_picker_dual #channel_history_au_time_picker_start_hour {
	top: 150px;
	left: 15px;
}

