.date-container {
	position: relative;
}

.date-icon-from {
	position: absolute;
	background:url("../../images/date_icon.png");
	width: 16px;
	height: 16px;
	top: 9px;
	left: 120px;
	z-index: 2;
}

.date-icon-to {
	position: absolute;
	background:url("../../images/date_icon.png");
	width: 16px;
	height: 16px;
	top: 9px;
	left: 120px;
	z-index: 99;
}

#date-input-from {
	width: 150px;
	height: 35px;
	color: rgb(121, 121, 121);
}

#date-input-to {
	width: 150px;
	height: 35px;
	color: rgb(121, 121, 121);
}

#search-container {
	position: relative;
}

#search-icon {
	position: absolute;
	background:url("../../images/invit_search_icon.png");
	width: 25px;
	height: 25px;
	top: 5px;
	left: 270px;
	z-index: 99;
}

#search-input {
	height: 35px;
	width: 300px;
	border: 1px #cfcfcf solid;
	padding-left: 20px;
	color: rgb(121, 121, 121);
}

#meeting-invit {
	width: 100%;
	height: 100%;
	font-family: Microsoft YaHei,微软雅黑,MS Song,宋体;
}

#find-records {
	height: 60px;
	width: 100%;
}

#find-records td {
	text-align: center;
}

#meeting-invit-list-body {
	height: 90%;
	font-size: 14px;
	border: 1px rgb(226, 226, 226) solid;
	background-color: rgb(242, 242, 242);
    padding: 20px 10px 20px 20px;
    box-sizing: border-box;
}

#meeting-invit-list-body .column0 {
	width: 22%;
}

#meeting-invit-list-body .column1 {
	width: 11%;
}

#meeting-invit-list-body .column2 {
	width: 11%;
}

#meeting-invit-list-body .column3 {
	width: 11%;
}

#meeting-invit-list-body .column4 {
	width: 30%;
}

#meeting-invit-list-body .column5 {
	width: 15%;
}

.record-ctrl-icons {
	text-align: center;
	width: 100%;
}

.record-ctrl-icons td{
	width: 10%;
}

#from-date-selector {
	position: absolute;
	z-index: 99;
	background-color: white;
}

#to-date-selector {
	position: absolute;
	z-index: 99;
	background-color: white;
	left: -150px;
}


.date-selector .td-date-title {
	color: rgb(121, 121, 121);
	border: rgb(226, 226, 226) solid;
	border-width: 0px 1px 1px 0px;
	text-align: center;
	height: 9%;
	font-size: 8pt;
}

.date-selector .non-curr-month {
	color: rgb(210,210,210);
	border: rgb(226, 226, 226) solid;
	border-width: 0px 1px 1px 0px;
	text-align: center;
	font-size: 10pt;
}

.date-selector .curr-month {
	color: rgb(121, 121, 121);
	border: rgb(226, 226, 226) solid;
	border-width: 0px 1px 1px 0px;
	text-align: center;
	font-size: 10pt;
}

.date-selector .curr-month:hover {
	cursor: pointer;
}

.date-selector .ctrl:hover {
	cursor: pointer;
}

.date-selector #ctrl-title {
	width: 300px;
}

.data-selector #ctrl-title td {
	width: 30px;
}

.date-selector #ctrl-title tr td:nth-child(3){
	width: 100px;
}

.date-selector #ctrl-title tr td:nth-child(4){
	width: 80px;
}

.date-selector #grid {
	border: rgb(203, 203, 203) solid;
	border-width: 1px 0px 0px 1px;
	width: 300px;
	height: 200px;
}

.date-selector #grid td {
	width: 14%;
}









