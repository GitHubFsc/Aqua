.CMS_ztree * {padding:0; margin:0; font-size:14px; font-family:'宋体';color:#797979}
.CMS_ztree {margin:0; padding:5px; color:#333}
.CMS_ztree li{position:relative;padding:0; margin:0; list-style:none; line-height:40px; text-align:left; white-space:nowrap; outline:0}
.CMS_ztree li ul{ margin:0; padding:0 0 0 18px}
.CMS_ztree li ul .level1 ul{ background:url(images/zTree/dash.png) 0 0 repeat-y;margin-left:7px;position:relative}
.CMS_ztree li ul .level2 ul{ background:url(images/zTree/dash.png) 0 0 repeat-y;margin-left:10px}
.CMS_ztree li a.level0,.CMS_ztree li a.level1,.CMS_ztree li a.level2,.CMS_ztree li a.level3,.CMS_ztree li a.level4{padding:1px 3px 0 0; margin:0; cursor:pointer; height:40px; color:#333; background-color: transparent;
	text-decoration:none; vertical-align:top; display: inline-block}
.CMS_ztree li a.level0{
	width: calc(100% + 28px);
	margin-left: -32px;
}
.CMS_ztree li a.level1{
	width: calc(100% - 19px);
}
.CMS_ztree li a.level2{
	width: calc(100% - 23px);
}
.CMS_ztree li a.level3{
	width: calc(100% - 23px);
}
.CMS_ztree li a.level4{
	width: calc(100% - 26px);
}
