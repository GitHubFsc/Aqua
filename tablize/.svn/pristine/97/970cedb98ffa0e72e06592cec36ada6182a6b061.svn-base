/**dialog model***/
.asset_import_method_selector .styled-selector-inputbox {
	border:1px solid rgb(211, 211, 211);
	padding-left:13px;
	border:2px #eaeef0 solid;
	border-radius:8px;
	color:#828282;
	font-size:12px;
	font-family:'宋体'
}
.asset_import_method_selector .styled-selector-dropbox td{
	padding-left:13px;
	color:#828282;
	font-size:12px;
	font-family:'宋体'
}

.asset_import_method_selector .styled-selector-dropbox{
	border: 2px solid #eaeef0;
	border-top:0;
	z-index:2;
	background-color:white!important
}

.asset_import_stuptact_styled_selector_down_arrow,.asset_import_stuptact_styled_selector_up_arrow{
    background-image: url("../../images/asset_import/down_arrow.png"); 
		background-repeat: no-repeat;
		background-position:center center;
    height: 13px;
    width: 20px;
}
.asset_import_stuptact_styled_selector_up_arrow{
	transform: rotate(180deg);
}
.asset_import_stuptact_styled_selector_expand{
	border-bottom-right-radius:0px!important;
	border-bottom-left-radius:0px!important;
}
.asset_import_border_div{
	border:2px #eaeef0 solid;
	height:32px;
	line-height:32px;
	font-size:12px;
	box-sizing: border-box;
	border-radius:8px
}
.asset_import_input_label{
	text-align:left;
	vertical-align:middle;
	height:32px;
	line-height:32px;
	display:inline-block;
	text-align:left;
	font-size:16px
}
.asset_import_panel_page_add_btn {
    background-color: #2ea2d7;
    background-image: url("../images/users/panel_add_btn_icon.png");
    background-position: 12px 10px;
    background-repeat: no-repeat;
    border: 1px solid #597f89;
    border-radius: 3px;
    color: white;
    cursor: pointer;
    font-size: 16px;
    font-weight: bold;
    height: 36px;
    line-height: 36px;
    padding-left: 36px;
    width: 88px;
}
.asset_import_button,.asset_import_grey_words,.asset_import_button_gray{
	height:38px;
	line-height:38px;
}
.asset_import_button{
	background-color:#2ea2d7;
	border:1px #597f89 solid;
	text-align:center;
	color:#ffffff;
	width:148px;
	font-size:16px;
	cursor:pointer
}
.asset_import_button_gray{
	background-color:#cccccc;
	border:1px #b4b4b4 solid;
	text-align:center;
	color:#ffffff;
	width:148px;
	font-size:16px;
}
.asset_import_grey_words{
	font-size:14px;
	color:#999999
}
.asset_import_title_container{
	background-color:#12a0ea;
  height:36px;
  line-height:36px;
	color:#ffffff;
	padding-left:14px;
	width:calc(100% - 38px);
}
.asset_import_title_container_thread_id{
	color: rgb(255, 255, 255);
	background-color: #45d1f4;
	text-align: center; 
	overflow: hidden;
	word-break: keep-all;
	white-space: nowrap; 
	text-overflow: ellipsis;
	height: 31px; 
}
.asset_import_title_container_id{
	background-color: white;
  color: #797979;
  height: 33px;
  overflow: hidden;
  position: relative;
  text-align: center;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-break: keep-all;
}

.asset_import_border_div .calendar-nav-title,.asset_import_border_div .calendar-dates-view,.asset_import_border_div .calendar-month-label,.asset_import_border_div .calendar-year-label {
  font-size: 14px;
	font-family:'微软雅黑'
}


.asset_import_back_btn {
	background-color: rgb(46, 162, 215);
	border: 1px solid rgb(89, 127, 137);
	border-radius: 2px;
	color: #fff;
	cursor: pointer;
	display: block;
	float: left;
	font-family: "Microsoft YaHei";
	font-size: 16px;
	font-weight: bold;
	height: 40px;
	line-height: 40px;
	margin-top: 10px;
	padding: 0 26px 0 18px;
	margin-left:12px
}

.asset_import_back_btn img {
	float: left;
	margin-right: 20px;
	margin-top: 8px;
}

.asset_import_header_title{
	border-left: 1px solid #b5b7c6;
	float: left;
	margin-left: 10px;
	margin-top: 10px;
	padding: 10px 10px 10px 11px;
	color:#848484;
	font-family:'微软雅黑'
}

.asset_import_button_style{
	background-color:#2ea2d7;
	height:28px;
	line-height:28px;
	text-align:center;
	border:1px #e2e2e2 solid;
	width:108px;
	margin-top:3px;
	cursor:pointer
}

::-webkit-input-placeholder { /* WebKit browsers */
color:#999;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
color:#999;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
color:#999;
}
:-ms-input-placeholder { /* Internet Explorer 10+ */
color:#999;
}