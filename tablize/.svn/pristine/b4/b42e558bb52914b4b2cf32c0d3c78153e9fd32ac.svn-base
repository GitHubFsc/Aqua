<div class="wp_panel_dialog lecture_recommend_dialog">
  <div class="wp_panel_dialog_title lecture_recommend_dialog_title">
    <div class="wp_panel_dialog_title_label" id="cms_user-admin-user-dialog-title">{{CMS_NEW_CARD}}</div>
    <div class="wp_panel_dialog_close popup_dialog_clear"></div>
  </div>
  <div class="wp_panel_dialog_content" style="margin-top:-2px;font-family:'宋体';color:#727272">
		<div style="width:auto;margin-top:16px">
			<div style="overflow:hidden;margin-top:16px">
				<div style="float:left;width:242px;margin-left:38px">
					<div class="inputTitlestyle">{{CMS_CARD_ID}}</div>
					<input id="cms_app_admin_input_card_id" class="adv_input_region" type="text" style="margin-top:7px;width:100%">
				</div>
				<div style="float:right;width:242px;margin-right:96px">
					<div class="inputTitlestyle_contacter">{{CMS_CARD_NAME}}</div>
					<input id="cms_app_admin_input_card_name" class="adv_input_region" type="text" style="margin-top:7px;width:100%">
				</div>
			</div>
			<div style="overflow:hidden;margin-top:21px;margin-left:38px;margin-right:32px;">
				<div class="inputTitlestyle">{{CMS_ASSET_CARD_LAYOUT}}</div>
				<div class="cms_app_admin_layout_container" style="margin-top:3px">
					<div id="cms_app_admin_layout_container" style="visibility:hidden;width:100%;height:100%;height:calc(100% - 14px);"></div>
				</div>
			</div>
		</div>	
    <div class="lecture_recommend_dialog_cancel popup_dialog_clear" id="cms_user-admin-dialog-cancel" style="right:205px;bottom:18px">{{CMS_CANCEL}}</div>
    <div class="lecture_recommend_dialog_submit" id="cms_add_card_submit" style="right:34px;bottom:18px;">{{CMS_NEW_ADD}}</div>
  </div>
</div>