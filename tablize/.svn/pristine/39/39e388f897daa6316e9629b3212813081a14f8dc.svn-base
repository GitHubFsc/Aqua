<div class="_dialog_assetTagLib meta">
  <div class="_dialog_title">
    <div id="addTagPopup_title" class="_dialog_title_label"></div>
    <div class="_span"></div>
    <div class="_dialog_close popup_dialog_clear"></div>
  </div>
  <div class="_dialog_body">
    <div class="_dialog_row" style="margin-top:12px">
      <label for="" style="font-size:14px">{{ASSET_TAG_LIB_TAG_NAME}}</label>
    </div>
    <div class="_dialog_row" style="margin-top:-2px">
      <input id="addTagPopup_input" style="width:325px;padding-left:8px;color:#747474;border-color:#c7c7c7;height:30px" type="text">
    </div>
		<div class="_dialog_row" style="margin-top:2px">
      <label for="" style="font-size:14px">{{ASSET_TAG_LIB_YOUXIAOSHIJIAN}}</label>
    </div>
		<div id="addTagPopup_po_time_box" class="po-time-box" style="z-index:900;margin-left:30px">
				<div class="po-time-picker" id="addTagPopup_validTimeBegin"></div>
				<div class="po-time-picker" style="color:#8d8d8d;float:left;width:26px;height:32px;line-height:32px;text-align:center" id="addTagPopup_validTimeBegin">-</div>
				<div class="po-time-picker" id="addTagPopup_validTimeEnd"></div>
				<div class="addTagsForMulti_checkBox" id="addTagPopup_alwaysValid" style="float:left;margin-top:7px;margin-left:13px">
					<div class="addTagsForMulti_checkBox_son" style="display: none;"></div>
				</div>
				<div style="width:auto;margin-left:8px;line-height:28px;float:left;color:#747474;font-size:14px">
					{{ASSET_TAG_LIB_ALWAYSVALID}}
				</div>
		</div>
  </div>
  <div class="_dialog_foot" style="height:38px;line-height:38px">
    <div id="addTagPopup_cancel" class="btn _cancel">{{AUTH_MNG_DIALOG_CANCEL}}</div>
    <div id="addTagPopup_confirm" class="btn _confirm">{{AUTH_MNG_DIALOG_DETAIL_CONFIRM}}</div>
  </div>
</div>
