}

#oa-sum-day-datepicker, 
#oa-oaTrendDay-start-datepicker, #oa-oaTrendDay-end-datepicker, 
#oa-oaTrendHour-start-datepicker,
#oa-oaTrendMonth-start-datepicker, #oa-oaTrendMonth-end-datepicker, 
#oa-oaTrendWeek-start-datepicker, #oa-oaTrendWeek-end-datepicker {
  position: relative;
  width: 143px;
  height: 31px;
  z-index: 99;
  color: #848484;
}

.page-norm-calendar .calendar-nav-title {
  font-size: 14px;
}

.page-norm-calendar .calendar-dates-view {
  font-size: 14px;
}

