  position: relative;
  width: 86px;
  height: 86px;
  margin-top: 5px;
  margin-left: 40px;
}

.trend-num-up {
  /*color: #f5a4d6;*/
  color: #ff66cc;
}

.trend-num-up + .trend-num-indicator {
  display: inline-block;
  height: 10px;
  width: 8px;
  margin-left: 5px;
  background-image: url(../images/operateanalyze/trend-num-up-arrow.png);
  background-position: center;
  background-repeat: no-repeat;
}

.trend-num-down {
  /*color: #67cb3e;*/
  color: #33cc00;
}

.trend-num-down + .trend-num-indicator {
  display: inline-block;
  height: 10px;
  width: 8px;
  margin-left: 5px;
  background-image: url(../images/operateanalyze/trend-num-down-arrow.png);
  background-position: center;
  background-repeat: no-repeat;
}

.trend-num-equal {
  /*color: #bcbcbc;*/
  color: #666666;
}

.trend-num-equal + .trend-num-indicator {
  display: inline-block;
  height: 10px;
  width: 10px;
  margin-left: 5px;
  background-image: url(../images/operateanalyze/trend-num-equal-sl.png);
  background-position: center;
  background-repeat: no-repeat;

}

