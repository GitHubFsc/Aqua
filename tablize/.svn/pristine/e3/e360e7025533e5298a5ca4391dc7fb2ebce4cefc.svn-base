.page-rel-container {
  /*position: relative;
  width: 100%;
  min-height: 100%;*/
}

.page-rel-header {
  position: relative;
  height: 60px;
}

.page-rel-content {
  position: relative;
  width: 100%;
  background-color: #eff3f6;
  padding-top: 5px;
  padding-bottom: 25px;
}

.day-sum-block {
  position: relative;
  margin-left: 10px;
  margin-right: 10px;
  margin-top: 12px;
  background-color: #fff;
}

.day-sum-ititle {
  padding-top: 20px;
  padding-bottom: 20px;
}

.day-sum-ititle-main {
  display: inline-block;
  font-size: 16px;
  height: 19px;
  line-height: 19px;
  margin-left: 18px;
  margin-right: 10px;
  color: #4d4a49;
  vertical-align: middle;

}

.day-sum-ititle-tail {
  display: inline-block;
  color: #fff;
  background-color: #2dbce8;
  border-radius: 3px;
  height: 19px;
  line-height: 19px;
  font-size: 10px;
  padding: 0 10px;
  vertical-align: middle;
}

.day-sum-total-pcount, .day-sum-total-tcount {
  width: 87px;
  height: 42px;
  padding: 3px;
  box-shadow: 0 0 3px #f3f3f3;
  border: 1px solid #f3f3f3;
  background-repeat: no-repeat;
  background-position: 3px 38px;
}

.day-sum-total-pcount {
  /*background-image: url(../images/operateanalyze/day-sum-pct.png);*/
}

.day-sum-total-tcount {
  /*background-image: url(../images/operateanalyze/day-sum-tct.png);*/
}

.day-sum-total-label {
  color: #707070;
  font-size: 12px;
  line-height: 16px;
}

.day-sum-total-num {
  color: #1c1615;
  font-size: 14px;
  line-height: 16px;
  margin-top: 6px;
}

.day-sum-act-deg {
  width: 184px;
  height: 106px;
  padding: 6px 12px;
  box-shadow: 0 0 3px #f3f3f3;
  border: 1px solid #f3f3f3;
  background-repeat: no-repeat;
  background-position: 12px 80px;
  /*background-image: url(../images/operateanalyze/day-sum-adt.png);*/
}

.day-sum-act-deg-label {
  font-size: 20px;
  color: #707070;
  line-height: 26px;
}

.day-sum-act-deg-num {
  font-size: 20px;
  color: #1a1615;
  line-height: 30px;
}

.day-sum-chart {
  position: relative;
  width: 1200px;
  height: 370px;
}

.day-sum-list {
  position: relative;
  width: 1200px;
  height: 370px;
  border-top: 1px solid #e1e1e1;
  overflow: auto;
}

.day-sum-data-list {
  table-layout: fixed;
}

.day-sum-data-list td {
  /*height: 46px;*/
  height: 40px;
  padding-left: 10px;
  white-space: nowrap;
}

.day-sum-data-list tr:nth-child(2n-1) {
  background-color: #f9f9f9;
}

.day-sum-data-list tr:nth-child(2n) {
  background-color: #fff;
}

.day-sum-data-list tr:nth-child(2n-1) td {
  /*color: #f5a4d6;*/
}

.day-sum-data-list tr:nth-child(2n) td {
  /*color: #67cb3e;*/
}

.day-sum-data-list tr:nth-child(1) td {
  color: #8696a9;
}

.day-sum-data-list tr td:nth-child(1) {
  color: #707070;
}

.day-sum-list-column-wide {
  width: 100px;
}

.day-sum-list-column-norm {
  width: 90px;
}

.day-sum-list .mCSB_container_wrapper {
  margin-bottom: 12px;
  margin-right: 10px;
}

.day-sum-list .mCSB_scrollTools {
}

.day-sum-list .mCSB_scrollTools .mCSB_draggerRail {
  background-color: #e8edef;
  border-radius: 0;
}

.day-sum-list .mCSB_scrollTools .mCSB_dragger .mCSB_dragger_bar {
  background-color: #a6dff1;
}

.day-sum-list .mCSB_scrollTools .mCSB_dragger:hover .mCSB_dragger_bar {
  /*background-color: #a6dff1;*/
  background-color: #2dbce8;
}

.day-sum-list .mCSB_scrollTools .mCSB_dragger.mCSB_dragger_onDrag .mCSB_dragger_bar {
  /*background-color: #a6dff1;*/
  background-color: #11a8d7;
}

.day-sum-list .mCSB_scrollTools.mCSB_scrollTools_horizontal .mCSB_draggerRail {
  height: 10px;
}

.day-sum-list .mCSB_scrollTools.mCSB_scrollTools_horizontal .mCSB_dragger .mCSB_dragger_bar {
  height: 10px;
}

.day-sum-list .mCSB_scrollTools.mCSB_scrollTools_vertical .mCSB_draggerRail {
  width: 10px;
}

.day-sum-list .mCSB_scrollTools.mCSB_scrollTools_vertical .mCSB_dragger .mCSB_dragger_bar {
  width: 10px;
}

