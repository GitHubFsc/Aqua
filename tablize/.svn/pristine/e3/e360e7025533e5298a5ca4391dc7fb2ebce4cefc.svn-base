}

#oa-sum-day-main-tcactive {
  background-image: url(../images/operateanalyze/oa-sum-day-main-3.png);
}

#oa-sum-day-main-pcactive {
  background-image: url(../images/operateanalyze/oa-sum-day-main-4.png);
}

.oa-sumday-layout-right {
  display: inline-block;
  margin-left: 26px;
  width: 660px;
  vertical-align: top;
}

.oa-sum-day-main-label {
  font-size: 16px;
  margin-left: 8px;
  line-height: 30px;
}

.oa-sum-day-main-num {
  font-size: 22px;
