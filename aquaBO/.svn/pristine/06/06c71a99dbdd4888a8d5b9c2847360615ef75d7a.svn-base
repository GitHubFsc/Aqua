  .trendAnalysis_checkbox {
    height: 30px;
    line-height: 30px;
    display: inline-block;
    margin-right: 26px;
  }
  .trendAnalysis_checkbox .checkbox {
    font-size: 0;
    height: 30px;
    position: relative;
    width: 16px;
    margin-right: 12px;
    float: left;
    line-height: 30px;
  }
  .trendAnalysis_checkbox .checkbox:after {
    content: "";
    display: inline-block;
    height: 100%;
    vertical-align: middle;
  }
  .trendAnalysis_checkbox .checkbox input {
    display: none;
  }
  .trendAnalysis_checkbox .checkbox label {
    cursor: pointer;
    width: 14px;
    height: 14px;
    line-height: 14px;
    background: white;
    border: 1px solid #ddd;
    display: inline-block;
    position: relative;
    vertical-align: middle;
  }
  .trendAnalysis_checkbox .checkbox label:after {
    opacity: 0.2;
    content: '\2002';
    position: absolute;
    width: 10px;
    height: 10px;
    background: grey;
    top: 2px;
    left: 2px;
  }
  .trendAnalysis_checkbox .checkbox label:hover::after {
  }
  .trendAnalysis_checkbox .checkbox input:checked + label:after {
    background: #32BEEA;
    opacity: 1;
  }
  .trendAnalysis_checkbox .checkbox input[type=checkbox] + label {
    border-radius: 0px;
  }
  .trendAnalysis_checkbox .checkbox input[type=checkbox] + label:after {
    border-radius: 0px;
  }
  .trendAnalysis_checkbox .checkbox input[type=radio] + label {
    border-radius: 7px;
  }
  .trendAnalysis_checkbox .checkbox input[type=radio] + label:after {
    border-radius: 6px;
  }
  .trendAnalysis_checkbox .checkboxLabel {
    height: 30px;
    line-height: 30px;
    font-size: 12px;
    color: #828282;
    overflow: hidden;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-left:28px;
  }
