  <div class="wp_panel_dialog_content">
    <table class="user-admin-dialog-layout" cellpadding="0" cellspacing="0" style="margin-top:2px">
      <tbody>
        <tr><td style="width:298px;">
          <label for="" class="user-admin-layout-label" >{{SYS_USER_USER_NAME}}</label>
          <input type="text" class="user-admin-layout-in" id="user-admin-user-name" style="height:30px"/>
        </td><td style="width:42px;"></td><td style="width:298px;">
          <label for="" class="user-admin-layout-label"></label>
