.program_update {}
.program_update .tab {
  width: 106px;
  height: 34px;
  line-height: 34px;
  border: 1px solid #d1d1d1;
  border-radius: 2px;
  text-align: center;
  font-size: 16px;
  color: #797979;
  margin: 0 1px 0 0 !important;
  cursor: pointer;
}
.program_update .tab.active {
  border-color: #83e0ff;
  color: #fff;
  background-color: #aae7fb;
}
.program_update .date_picker {
  width: 148px;
  height: 33px;
}
