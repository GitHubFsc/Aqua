#sucai_container {
    height: 100%;
}
#sucaizu_container {
     height: 100%;
 }

#tagManage_container {
    height: 100%;
}

/***************** head ***************/

.sucai_head {
    padding: 8px 0;
    border: 1px solid white;
    padding: 8px 0;
    position: relative;
    height: 40px;
    top: 2px;
}
.sucai_head_left{
    float: left;
    width: 80%;
}

.sucai_head_right {
    float: right;
    margin-right: 12px;
}

.sucai_add_class_btn {
    border-radius: 2px;
    height: 38px;
    line-height: 38px;
    background: #8fc31f;
    color: white;
    padding-left: 10px;
    padding-right: 20px;
    cursor: pointer;
}
.sucai_add_class_btn.page_button_inactive {
    cursor: default;
    opacity: 0.4;
}

.sucai_head_center {
    zoom: 1;
    height: 38px;
    margin-left: 10px;
    border-right: 1px solid #b5b7c6;
    padding-right: 10px;
    margin-right: 10px;
    float: left;
}

.sucai_head_center li {
    cursor: pointer;
    list-style: none;
    float: left;
    width: 87px;
    height: 38px;
    text-align: center;
    line-height: 38px;
    border: 1px solid #e2e2e2;
    color: black;
    font-weight: bold;
    font-size:16px;
    font-family:宋体;
    border-radius: 2px;
    background: linear-gradient(#f7f7f7, #e4e4e4);
    background: -ms-linear-gradient(#f7f7f7, #e4e4e4);
}

.sucai_head_center li.sucai_menu_selected {
    color: white;
    font-weight: bold;
    background: #75b8d6;
    border: 1px solid #4488a7;

    border-right: none;
}
.sucai_content_container{
    margin-left: 7px;
    margin-right: 7px;
    border: 1px solid #ededdb;
    padding: 2px;
    background-color: #fcfcf2;
}
/********创建对话********/

.sucai_create_dialog {
    position: relative;
    width: 100%;
    height: 100%;
    border: 1px solid rgb(93,161,192);
    background-color: white;
    font-family: 宋体;
    font-size:14px;
    color:#727272;
}

.sucai_dialog_title {
    width: 100%;
    height: 34px;
    background-color: rgb(93,161,192);
}

.sucai_dialog_title_text {
    position: absolute;
    left: 10px;
    height: 34px;
    line-height: 34px;
    color: white;
    cursor: default;
    color: white;
}

.sucai_dialog_close {
    position: absolute;
    background-image: url(../../../images/word_library_dialog.png);
    background-position: center;
    background-repeat: no-repeat;
    width: 18px;
    height: 18px;
    top: 8px;
    right: 10px;
    cursor: pointer;
}
.sucai_dialog_create_name, .sucai_dialog_create_others, .sucai_dialog_create_source,.sucai_dialog_create_opition{
    margin-top:16px;
    margin-left:10px;
}
.sucai_dialog_create_opition{
    float:right;
}

.sucai_dialog_create_name_title_span{
    padding-bottom: 6px;
}
.sucai_dialog_create_input{
    height: 30px;
    width: 300px;
    line-height: 30px;
    border: 1px #e2e2e2 solid;
    font-size: 14px;
    padding-left: 7px;
    vertical-align: middle;
    outline: none medium;
    color:#8a8a8a
}
.sucai_dialog_create_others{
    width:100%;
}
.sucai_dialog_create_size{
    float:left;
    width:50%;
}
.sucai_dialog_twices{
    float:left;
    width:50%;
}
.sucai_dialog_thirds{
    float:left;
    width:33%;
}
.sucai_dialog_create_morenguanggaowei{
    float:left;
    width:33%;
}
.sucai_dialog_create_isCDN{
    float:left;
    width:17%;
    height:20px;
}
.sucai_dialog_create_isCDN .check_box{margin: 8px;height: 14px;width: 14px;border: 1px solid #E2E2E2;cursor: pointer;}
.sucai_dialog_create_isCDN .check_box.checked {
    background-image: url(../image/adPolicyManage/check.png);
}
.sucai_dialog_create_size .check_box{margin: 8px;height: 14px;width: 14px;border: 1px solid #E2E2E2;cursor: pointer;}
.sucai_dialog_create_size .check_box.checked {
    background-image: url(../image/adPolicyManage/check.png);
}

.sucai_dialog_clear{
    clear : both;
}
.sucai_dialog_content_IMG{
    float: left;
    width: 50%;
    text-align: center;
    margin-top: 40px;
}
.sucai_dialog_content_text{
    float: left;
    width: 50%;
    text-align: left;
    height: 170px;
    line-height: 170px;
    font-size: 16px;
    font-family: 宋体;
    width:200px;/*对宽度的定义,根据情况修改*/
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.sucai_dialog_img_content{
    width:100%;
    height:calc(100% - 36px)
}

.sucai_dialog_create_move_sucai{
    height:200px;
    margin-left:10px;
    width:95%;
    font-size:14px;
    font-family: 宋体;
}

.sucai_dialog_create_move_sucai_one{
    float: left;
    width: 40%;
    height: 349px;
}
.sucai_dialog_create_move_sucai_two{
    float: left;
    width: 20%;
    height: 349px;
}

.sucai_dialog_create_move_sucai_two div{
    width:70%;
    margin-left: 15%;
    height:30px;
    margin-top:16px;
    background:rgb(94,164,192);
    color:#fff;
    border:1px solid #e2e2e2;
    text-align: center;
    line-height: 30px;
    cursor: pointer;
}
.sucai_dialog_create_move_sucai_three{
    float: left;
    width: 40%;
    height: 349px;
}

#ViewSucaiZuContent .mCSB_inside > .mCSB_container{
    margin-right: 2px;
}
/*************对话框的确认取消***************/
.option_bottom{
    width: 92px;
    height: 31px;
    background-color: #88c5e0;
    line-height: 31px;
    text-align: center;
    color: white;
    float: left;
    font-size:14px;
    font-family:宋体;
    margin-right: 16px;
    border:1px solid #6faac4;
    cursor: pointer;
}
.confirm_bottom{
    background-color: #5da1c0;
    border:1px solid  #597f89;
}
.confirm_bottom.disabled{
    background-color: #a9a9a9;
    cursor: default;
}

/***************** selected ***************/

.select-box {
    float: left;
    position: relative;
}
.select-input {
    border: 1px #e2e2e2 solid;
    font-size: 16px;
    color: #a9a9a9;
    padding-left: 7px;
    vertical-align: middle;
    outline: none medium;
}
.select-status .select-header {
    width: 88px;
    height: 38px;
    border: 1px solid #eaeaea;
    padding-right: 26px;
    background: url(../image/sucai/arrow.png) no-repeat #f9f9f9;
    background-position: 95% 13px;
    line-height: 38px;
    text-indent: 14px;
    font-size: 16px;
    color: #a9a9a9;
    cursor: pointer;
    overflow: hidden;
}

.select-status .select-arrow {
    background-position: 95% -9px;
}

.select-status .select-content {
    border-left: 1px solid #eaeaea;
    border-right: 1px solid #eaeaea;
    top: 38px;
    left: 0;
    z-index: 99;
    display: none;
    background-color: #f9f9f9;
    position: absolute;
}

.select-status .select-content li {
    list-style: none;
    width: 114px;
    height: 30px;
    margin: 0;
    border-bottom: 1px solid #eaeaea;
    line-height: 38px;
    text-indent: 14px;
    background-color: #f9f9f9;
    font-size: 16px;
    color: #a9a9a9;
    cursor: pointer;
}

.sucai_search_Container{
    overflow: hidden;
    float: left;
    width: 207px;
    margin-left: 25px;
}

.sucai_list_search{
    float: left;
    height: 36px;
    width: 195px;
    line-height: 36px;
    border: 1px #e2e2e2 solid;
    font-size: 12px;
    padding-left: 7px;
    vertical-align: middle;
    outline: none medium;
    background:#f4f4f4;
}

.sucai_search_icon {
    float: left;
    height: 36px;
    width: 22px;
    background-image: url("../image/sucai/search_icon_prepare.png");
    background-repeat: no-repeat;
    margin-left: -13%;
    margin-top: 4.5%;
    cursor: pointer;
}

.sucai_list_search_botton_div{
    float: left;
    height: 30px;
    width: 50px;
    border: 1px solid #FFF;
    background: RGB(123,183,208);
    cursor: pointer;
    color: white;
    text-align: center;
    line-height: 30px;
    margin-top: 2px;
    position: relative;
    left: -52px;
}

.sucai_dialog_create_search{
    margin-top: 16px;
    margin-left: 10px;
    width:760px;
    background-color: rgb(92,161,192);
    height: 36px;
}
.sucai_dialog_create_search_title{
    float:left;
    width:80px;
    height:36px;
    line-height:36px;
    color:white;
    text-align: center;
}

.sucai_dialog_create_area{
    float:left;
    width:100px;
    height:30px;
    line-height:30px;
    margin-top:3px;
    margin-right:10px;
    color:white;
    text-align: left;
    background: white;
    display:none;
}
/***************table**************/
.sucai_smallWord_in_td{
    font-size: 14px;
    color: #3c94bb;
    margin-left: 18px;
    cursor: pointer;
    font-family: 宋体;
    text-decoration : underline;
}

/***************马强组件的css**********************/
.styled-flow-list-container  table {
    table-layout: fixed;
    width: 100%;
    border-spacing: 1px;
    background-color: rgb(226,226,226);
}

.styled-flow-list-container  table > tbody > tr > th {
    background-color: #45d1f4;
    text-align: center;
    height: 35px;
}
.styled-flow-list-content-container{
    height:300px
}
.styled-flow-list-container  table > tbody > tr > th:nth-child(1) {
    width: 40px;
}

.styled-flow-list-container  table > tbody > tr > th:nth-child(2) {
    width: 40px;
}

.styled-flow-list-container  table > tbody > tr > th:nth-child(3) {
    color: white;
}

.styled-flow-list-container  table > tbody > tr > td {
    height: 36px;
    color: rgb(121,121,121);
    background-color: white;
}

.styled-flow-list-container  table > tbody > tr > td:nth-child(1) {
    width: 40px;
    text-align: center;
}
.styled-flow-list-container  table > tbody > tr > td:nth-child(2) {
    width: 40px;
    text-align: center;
}
.styled-flow-list-container  table > tbody > tr > td:nth-child(3) {
    text-align: left;
    padding-left: 20px;
}

.mCS-my-theme.mCSB_scrollTools .mCSB_dragger .mCSB_dragger_bar{ background-color: grey; }
.mCS-my-theme.mCSB_scrollTools .mCSB_draggerRail{ background-color: white; }
.glyphicon-ok {
    position: absolute;
    display: inline-block;
    right: 15px;
    margin-top: 10px;
    background-image: url(../image/adPolicyManage/check.png);
    width: 15px;
    height: 15px;
}

.mask_transition{
    position: fixed;
    z-index: 1001;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.6);
}