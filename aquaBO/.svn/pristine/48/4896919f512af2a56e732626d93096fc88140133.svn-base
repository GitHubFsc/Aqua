* {margin: 0;padding: 0;}
.product_offering{height: 100%;width: 100%;}
.product_offering._flex{display: -webkit-box;display: -ms-flexbox;display: flex;-webkit-box-orient: vertical;-webkit-box-direction: normal;-ms-flex-flow: column;flex-flow: column;}
.product_offering .header{height: 60px;display: -webkit-box;display: -ms-flexbox;display: flex;-webkit-box-orient: horizontal;-webkit-box-direction: normal;-ms-flex-flow: row;flex-flow: row;-webkit-box-align: center;-ms-flex-align: center;align-items: center;background-color: #fff;}
.product_offering .header .btn{color: #FFFFFF !important;border: 1px solid #597F89;border-radius: 3px;background-color: #2EA2D7;cursor: pointer;height: 32px;}
.product_offering .header .header_item{height: 34px;margin: 14px 0;line-height: 34px;font-size: 16px;font-weight: bold;color: #787878;z-index: 1;border-radius: 3px;}
.product_offering .header .header_item.span{-webkit-box-flex: 1;-ms-flex: 1;flex: 1;}
.product_offering .header .header_item input{padding-left: 10px;}
.product_offering .header .logo{padding-left: 30px;border: 1px solid transparent;background-image: url(../../images/offering/logo.png);background-position: 10px center;background-repeat: no-repeat;}
.product_offering .header .split{width: 31px;border: 1px solid transparent;background-image: url(../../images/offering/split_bar.png);background-position: center;background-repeat: no-repeat;}
.product_offering .header .search_bar{width: 300px;border: 1px solid #EAEAEA;background-color: #F4F4F4;display: -webkit-box;display: -ms-flexbox;display: flex;}
.product_offering .header .search_bar .search_input{background-color: transparent;border: 1px solid transparent;-webkit-box-flex: 1;-ms-flex: 1;flex: 1;padding-left: 10px;}
.product_offering .header .search_bar .search_btn{width: 16px;height: 16px;margin: 9px 11px;background-image: url(../../images/offering/search_btn.png);background-position: center;background-repeat: no-repeat;cursor: pointer;}
.product_offering .header .dropdown{width: 131px;margin: 0 5px;}
.product_offering .header #create.btn{width: 113px;margin: 0 5px;background-image: url(../../images/offering/create.png);background-repeat: no-repeat;padding-left: 37px;background-position: 10px 9px;}
.product_offering .select-status .select-header, .dialog .select-status .select-header{width: 100%;line-height: 30px;padding-right: 0;border: 1px solid #e4e4e4;}
.product_offering .select-status .select-content, .dialog .select-status .select-content{width: 100%;}
.product_offering .select-status .select-content li, .dialog .select-status .select-content li{width: 100%;}

.product_offering .body{-webkit-box-flex:1;-ms-flex:1;flex:1;border: 1px solid #EDEDDB;background-color: #FCFCF2;position: relative;display: -webkit-box;display: -ms-flexbox;display: flex;-webkit-box-orient: vertical;-webkit-box-direction: normal;-ms-flex-flow: column;flex-flow: column;font-size: 14px;}
.product_offering .body #list_table{/**-webkit-box-flex:1;-ms-flex:1;flex:1;**/margin: 6px 8px;height: 615px;}
.product_offering .body #list_table tbody td:nth-child(7){overflow: visible !important;}
.product_offering .body #list_table span{display: -webkit-box;display: -ms-flexbox;display: flex;-webkit-box-pack: center;-ms-flex-pack: center;justify-content: center;}
.product_offering .body #list_table span a{margin: 0 5px;color: #2EA2D7;cursor: pointer;display: block;height: 24px;line-height: 24px;border: 1px solid transparent;}
.product_offering .body #list_table span a.disable{color: #797979;cursor: default;}
.product_offering .body #list_table span a.more{position: relative;width: 38px;background-image: url(../../../images/offering/edit_down_arrow.png);background-repeat: no-repeat;background-position: 27px center;text-align: left;}
.product_offering .body #list_table span a.more .more_detail{position: absolute;border: 1px solid #CCD3D4;right: -1px;top: -1px;width: 103px;z-index: 1;background-color: #FFFFFF;text-align: right;display: none;}
.product_offering .body #list_table span a.more.on .more_detail{display: block;}
.product_offering .body #list_table span a.more .more_detail > div{border-bottom: 1px solid #E1E1E1;padding-right: 5px;display: block;}
.product_offering .body #list_table span a.more .more_detail > div:last-child{border-bottom: none;}
.product_offering .body #list_table span a.more .more_detail > div:first-child{background-image: url(../../../images/offering/edit_up_arrow.png);background-repeat: no-repeat;background-position: 92px center;padding-right: 15px;}
.product_offering ._flex{display: -webkit-box;display: -ms-flexbox;display: flex;background-color: #fff;}
.product_offering ._flex._column, .product_offering ._flex._column{-webkit-box-orient: vertical;-webkit-box-direction: normal;-ms-flex-flow: column;flex-flow: column;}
.product_offering ._flex._row, .product_offering ._flex._row{-webkit-box-orient:horizontal;-webkit-box-direction:normal;-ms-flex-flow:row;flex-flow:row;}
.product_offering .main_page, .product_offering .product_policy_dialog {height: 100%;width: 100%;}

.product_policy_dialog .body {color: #727272;}
.product_policy_dialog .body input, .product_policy_dialog .body textarea {color: #727272;padding-left: 10px;}
.product_policy_dialog .body input.datepicker-input {padding-left: 10px;}
.product_policy_dialog .body .col {-webkit-box-flex: 1;-ms-flex: 1;flex: 1;padding-left: 45px;}
.product_policy_dialog .body .col > label {padding-top: 18px;padding-bottom: 6px;display: block;}
.product_policy_dialog .body .col .group {background-color: #f3f3f3;width: calc(100% - 44px);padding-left: 9px;margin-right: 20px;padding-right: 15px;padding-bottom: 15px;}
.product_policy_dialog .body .col .group .row {padding-top: 14px;height: auto;}
.product_policy_dialog .body .col .group .col1 {width: 288px;}
.product_policy_dialog .body .col .group .col2 {-webkit-box-flex: 1;-ms-flex: 1;flex: 1;padding-left: 70px;}
.product_policy_dialog .body .col .group .row .value_input {width: 100%;}
.product_policy_dialog .body .col .group .combo {width: auto;}
.product_policy_dialog .body .col .group input {width: auto;-webkit-box-flex: 1;-ms-flex: 1;flex: 1;border: 1px solid #e2e2e2;}
.product_policy_dialog .body .col .group label {display: block;line-height: 26px;width: auto;}
.product_policy_dialog .body .row {line-height: 32px;padding-top: 18px;-webkit-box-align: center;-ms-flex-align: center;align-items: center;}
.product_policy_dialog .body .spliter {border-left: 1px solid #e2e2e2;margin: 10px 0;}
.product_policy_dialog .body .row label {width: 93px;}
.product_policy_dialog .body .row input {height: 32px;}
.product_policy_dialog .body .row #effective_date_picker {width: 298px;}
.product_policy_dialog .body .row #expiration_date_picker {width: 298px;}
.product_policy_dialog .body .row .value_input {width: 298px;height: 32px;border: 1px solid #e2e2e2;padding-left: 10px;}
.product_policy_dialog .body .value_input[disabled]{background-color: #f0f0f0;}
.product_policy_dialog .body input.datepicker-input[disabled]{background-color: #f0f0f0;}
.product_policy_dialog .body .row .combo {width: 298px;}
.product_policy_dialog .foot{-webkit-box-pack: end;-ms-flex-pack: end;justify-content: flex-end;padding-top: 8px;padding-bottom: 9px;}
.product_policy_dialog .foot .btn{height: 38px;border: 1px solid #597f89;width: 149px;margin-right: 15px;line-height: 38px;text-align: center;color: #ffffff;cursor: pointer;font-size: 16px;}
.product_policy_dialog .foot .btn.confirm{background-color: #2ea2d7;}
.product_policy_dialog .foot .btn.update{background-color: #2ea2d7;}
.product_policy_dialog .foot .btn.back{background-color: #2ea2d7;}
.product_policy_dialog .foot .btn.cancel{background-color: #5da1c0;}


.dialog{background-color: #FFFFFF;border: 1px solid #177ba9;height: calc(100% - 2px);}
.dialog ._flex, .dialog._flex{display: -webkit-box;display: -ms-flexbox;display: flex;}
.dialog ._flex._column, .dialog._flex._column{-webkit-box-orient: vertical;-webkit-box-direction: normal;-ms-flex-flow: column;flex-flow: column;}
.dialog ._flex._row, .dialog._flex._row{-webkit-box-orient:horizontal;-webkit-box-direction:normal;-ms-flex-flow:row;flex-flow:row;}
.dialog .title{height: 38px;background-color: #2ea2d7;color: #ffffff;padding-left: 10px;line-height: 38px;font-size: 18px;}
.dialog .title .dialog_close{width: 38px;background-image: url(../../images/offering/sucai_dialog_close.png);cursor: pointer;}
.dialog .image {background-repeat: no-repeat;background-position: center;}
.dialog .title .title_text{-webkit-box-flex:1;-ms-flex:1;flex:1;}
.dialog .body{-webkit-box-flex:1;-ms-flex:1;flex:1;padding-top: 8px;background-color: #ffffff;color: #727272;}
.dialog .body .row{display: -webkit-box;display: -ms-flexbox;display: flex;}
.dialog .body .col{padding-left: 40px;padding-right: 40px;-webkit-box-flex: 1;-ms-flex: 1;flex: 1;}
.dialog .body .col1{-webkit-box-flex:1;-ms-flex:1;flex:1;padding-left: 40px;padding-right: 60px;}
.dialog .body .col2{-webkit-box-flex:1;-ms-flex:1;flex:1;padding-right: 40px;padding-left: 60px;}
.dialog .body label{font-size: 14px;height: 26px;line-height: 26px;display: block;color: #727272;}
.dialog .body .value_input{padding-left: 10px;height: 30px;border: 1px solid #e4e4e4;border-radius: 2px;margin-bottom: 12px;background-color: #ffffff;line-height: 30px;}
.dialog .body .value_input[disabled]{background-color: #f0f0f0;}
.dialog .body input.value_input{width: 100%;}
.dialog .body input:disabled{background-color: #f0f0f0;}
.dialog .body .combo{height: 30px;background-color: #FFFFFF;margin-bottom: 12px;}
.dialog .body .group{background-color: #f3f3f3;width: 100%;}
.dialog .body .group .col1{padding-left:9px;}
.dialog .body .group .col2{padding-right:9px;}
.dialog .foot{height: 55px;-webkit-box-pack: end;-ms-flex-pack: end;justify-content: flex-end;}
.dialog .foot .btn{height: 38px;border: 1px solid #597f89;width: 149px;margin-right: 15px;line-height: 38px;text-align: center;color: #ffffff;cursor: pointer;font-size: 16px;}
.dialog .foot .btn.confirm{background-color: #2ea2d7;}
.dialog .foot .btn.update{background-color: #2ea2d7;}
.dialog .foot .btn.cancel{background-color: #5da1c0;}


.dialog.alert .body{background-image: url(../../images/offering/alert_icon.png);background-repeat: no-repeat;padding: 49px 54px 0 146px;background-position: 21px 34px;color: #797979;font-size: 19px;line-height: 35px;font-weight: bold;}
.dialog.policy #allow_repeat_purchase{height: 30px;background-color: #ffffff;}
.dialog input, .dialog textarea{color: #727272;}
.dialog .combo input{padding-left: 10px;border: 1px solid #e4e4e4;}
.dialog #ticket_num{height: 30px;width: 91px;border: 1px solid #e4e4e4;padding-left: 10px;margin-right: 13px;}
.dialog #ticket{width: 65px;}
.dialog #effective_date_picker input{padding-left: 10px;}
.dialog #expiration_date_picker input{padding-left: 10px;}


