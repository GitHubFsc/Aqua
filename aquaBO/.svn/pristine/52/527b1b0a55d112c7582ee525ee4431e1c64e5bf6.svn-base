<div class="_dialog detail_dialog">
  <div class="_dialog_title">
    <div class="_dialog_title_label">{{AUTH_MNG_QUERYTABLE_OPR_DETAIL}}</div>
    <div class="_dialog_close"></div>
  </div>
  <div class="_dialog_body">
    <div class="_dialog_row" style="height: auto;">
      <label for="">{{AUTH_MNG_DIALOG_DETAIL_USERID}}</label>
      <label for="">{{AUTH_MNG_DIALOG_DETAIL_TICKET}}</label>
    </div>
    <div class="_dialog_row">
      <input type="text" disabled id="card_id">
      <input type="text" disabled id="ticket_id">
    </div>
    <div class="_dialog_row" style="height: auto;">
      <label for="">{{AUTH_MNG_DIALOG_DETAIL_TYPE}}</label>
      <label for="">{{AUTH_MNG_DIALOG_DETAIL_ASSET}}</label>
    </div>
    <div class="_dialog_row">
      <input type="text" disabled id="asset_type">
      <input type="text" disabled id="asset_id">
    </div>
    <div class="_dialog_row" style="height: auto;">
      <label for="">{{AUTH_MNG_DIALOG_DETAIL_APPKEY}}</label>
      <label for="">{{AUTH_MNG_DIALOG_DETAIL_ORDER}}</label>
    </div>
    <div class="_dialog_row">
      <input type="text" disabled id="app_key">
      <input type="text" disabled id="order_id">
    </div>
    <div class="_dialog_row" style="height: auto;">
      <label for="">{{AUTH_MNG_DIALOG_DETAIL_CREATETIME}}</label>
      <label for="">{{AUTH_MNG_DIALOG_DETAIL_HOME}}</label>
    </div>
    <div class="_dialog_row">
      <input type="text" disabled id="create_time">
      <input type="text" disabled id="home_id">
    </div>
    <div class="_dialog_row" style="height: auto;">
      <label for="">{{AUTH_MNG_DIALOG_DETAIL_EFFECTIVETIME}}</label>
      <label for="">{{AUTH_MNG_DIALOG_DETAIL_EXPIRETIME}}</label>
    </div>
    <div class="_dialog_row">
      <input type="text" disabled id="effective_time">
      <input type="text" disabled id="expire_time">
    </div>
    <div class="_dialog_row" style="height: auto;">
      <label for="">{{AUTH_MNG_QUERYTABLE_CANCELAUTHTIME}}</label>
      <label for="">{{AUTH_MNG_QUERYTABLE_TYPE_CANCELAUTH}}</label>
    </div>
    <div class="_dialog_row">
      <input type="text" disabled id="event_time">
      <input type="text" disabled id="delete_type">
    </div>
    <div class="_dialog_row" style="height: auto;">
      <label for="">{{AUTH_MNG_DIALOG_DETAIL_STATE}}</label>
    </div>
    <div class="_dialog_row">
      <input type="text" disabled id="state">
      <input type="text" disabled style=" visibility: hidden">
    </div>
  </div>
  <div class="_dialog_foot">
    <div class="_btn _cancel">{{AUTH_MNG_DIALOG_DETAIL_CONFIRM}}</div>
  </div>
</div>
