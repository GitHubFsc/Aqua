/* 异常订单首页 */
.abnormalOrders .header .logo {padding-left: 20px;color: #494b58;}
.abnormalOrders .header .search_bar {background-color: #fff;}
.abnormalOrders .header .search_bar .search_input {background-color: #fff;}

/* 异常订单详单 */
.abnormalOrder {width: 100%;height: 100%;position: absolute;top: 0;right: 0;bottom: 0;left: 0;background-color: #fff;z-index: 99;cursor: default;}
.abnormalOrder .header .btn.back {width: 45px;height: 38px;line-height: 38px;padding-left: 50px;border-radius: 3px;background-image: url(../../../images/abnormal_orders/btn_back.png);background-position: 18px center;background-repeat: no-repeat;font-size: 16px;font-weight: bold;}
.abnormalOrder .header .logo {margin: 14px 0px;padding-left: 15px;background-image: url(../../../images/abnormal_orders/btn_left_gray.png);background-position: 0px center;color: #797979;font-size: 16px;}

/* alert弹框 */
._dialog.alert ._dialog_body {padding: 35px 15px 0 175px;background-position: 30px 45px;color: #797979;font-size: 20px;}
._dialog.alert ._dialog_foot {margin-bottom: 5px;}