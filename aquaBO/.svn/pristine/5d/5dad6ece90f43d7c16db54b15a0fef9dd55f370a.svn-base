{
	"name": "模板G",
	"layoutType": "grid",
	"width": 1740,
	"height": 400,
	"maxLineCount": 1,
	"maxRowCount": 6,
	"lineSpace": 0,
	"rowSpace": 0,
	"items": [
		{
			"type": "standard",
			"line": 1,
			"row": 1,
			"mergeRowCount": "6",
			"focusEnable": true,
			"defaultBgColor": "#80000000",
			"defaultFocusScale": 1.05,
			"defaultFocusBorderColor": "#ffffffff",
			"defaultFocusBorderWidth": 4,
			"defaultFocusBorderRadius": 1,
			"defaultFocusZ": 20,
			"dataItem": {
				"normal": {
					"type": "relative",
					"elements": [
						{
							"index": 0,
							"type": "image",
							"zIndex": 0,
							"top": 0,
							"left": 0,
							"width": -1,
							"height": -1,
							"url": "$posterUrl$",
							"alignParent": "bottom"
						},
						{
							"index": 1,
							"type": "image",
							"zIndex": 1,
							"top": 0,
							"left": 0,
							"width": -1,
							"height": 133,
							"url": "/img/jctj_text_bg_n2.png",
							"alignParent": "bottom"
						},
						{
							"index": 2,
							"type": "text",
							"zIndex": 2,
							"top": 0,
							"left": 10,
							"bottom": 10,
							"width": -1,
							"height": -2,
							"alignParent": "bottom",
							"textColor": "#ffffffff",
							"textSize": 30,
							"text": "$title$"
						}
					]
				},
				"focus": {
					"type": "relative",
					"elements": [
						{
							"index": 0,
							"type": "image",
							"zIndex": 0,
							"top": 0,
							"left": 0,
							"width": -1,
							"height": -1,
							"url": "$posterUrl$",
							"alignParent": "bottom"
						},
						{
							"index": 1,
							"zIndex": 1,
							"type": "shape",
							"top": 0,
							"left": 0,
							"width": -1,
							"height": 56,
							"alignParent": "bottom",
							"shapeBgColor": "#ffffffff"
						},
						{
							"index": 2,
							"type": "text",
							"zIndex": 2,
							"top": 0,
							"left": 10,
							"bottom": 10,
							"width": -1,
							"height": -2,
							"alignParent": "bottom",
							"textColor": "#ff010101",
							"textSize": 30,
							"text": "$title$"
						}
					]
				},
				"actionIntent": {
					"intentName": "OpenRecommand",
					"slots": {
						"type": "$type$",
						"folderFullPath": "$folderFullPath$",
						"templateId": "$templateId$",
						"pid": "$pid$",
						"paid": "$paid$",
						"r_object_name": "$title$"
					}
				}
			}
		}
	]
}