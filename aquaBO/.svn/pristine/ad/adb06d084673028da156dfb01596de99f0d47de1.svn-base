.user-admin-group-table-row {
	  display: -webkit-box;
	  display: -ms-flexbox;
	  display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
        -ms-flex-flow: row;
            flex-flow: row;
		border-bottom: 1px solid #e2e2e2;
}
.user-admin-group-table-row > div {
	border-right: 1px solid #e2e2e2;
	width: 41px;
	text-align: center;
}
.user-admin-group-table-row > div:last-child {
	padding-left: 23px;
	text-align: left;
	border-right: none;
	-webkit-box-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
}
.user-admin-group-table-row div.check_box {
	height: 10px;
	width: 10px;
	border: 1px solid #cbcbcb;
	border-radius: 1px;
	background-color: #fff;
	margin: calc(50% - 6px) calc(50% - 6px);
	cursor: pointer;
}
.user-admin-group-table-row div.check_box >div {
	width: 8px;
	height: 8px;
	margin: 1px;
}
.user-admin-group-table-row div.check_box.focus >div {
	background-color: #45d1f4;
}

