<!doctype html>
<html lang="zh">
<head>
	<meta charset="UTF-8" />
	<title>Document</title>
	<script type="text/javascript">
		var fileTime = new Date().getTime();

		function loadStyle(){
			var styles = [
				'style/index.css'
			];

			for(var i = 0, len = styles.length; i < len; i+=1){
				document.write('<link rel="stylesheet" type="text/css" href="' + styles[i] + '?t='+ fileTime +'" />');
			}
		}

		function loadScript(){
			var jses = [
				'config.js',
				'js/common/endecodebase64.js',
				'js/common/sha1.js',
				'js/common/keyvalue.js',
				'js/common/util.js',
				'js/app.js',
				'js/my.paas.js',
				'js/vtil.js',
				'js/index.js'
			];

			for(var i = 0, len = jses.length; i < len; i+=1){
				document.write('<script type="text/javascript" src="' + jses[i] + '?t=' + fileTime + '"><\/script>');
			}
		}

		loadStyle();
		loadScript();

	</script>
</head>
<body onload="init()">
	<div class="app">
		<div class="dialog">
			<div class="name-txt">年华应用审核发布预览</div>
			<div class="choice-row">
				<div class="choice-name">版本选择</div>
				<div class="choice-caro" id="ver-choice"></div>
			</div>
			<div class="choice-row">
				<div class="choice-name">应用选择</div>
				<div class="choice-caro" id="app-choice"></div>
			</div>
			<div class="btn-ok">选择进入</div>
		</div>
	</div>
</body>
</html>