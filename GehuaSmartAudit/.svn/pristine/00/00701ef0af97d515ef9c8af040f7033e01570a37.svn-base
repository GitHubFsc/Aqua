/* 更改styleable_list */
.poster_audit .list_table .styleable_list_controls {
    display: none;
}
.poster_audit .list_table .styleable_list_row .styleable_list_col {
    color: #777
}
.poster_audit .list_table .styleable_list_title .styleable_list_col_tag {
    height: 36px;
    color: #747474;
    font-size: 12px;
    font-weight: normal;
}
/*alert_dialog*/
#audit_alert_dialog .row {
    height: 40px;
    margin-bottom: 10px;
}
#audit_alert_dialog .row.textarea {
    height: 100px;
}
#audit_alert_dialog .result {
    width: 200px;
    height: 30px;
    line-height: 30px;
    padding-left: 15px;
    border: 1px solid #ccc;
    border-radius: 5px;
    background-color: #f5f5f5;
}
#audit_alert_dialog .result_pass {
    color: #03d40d;
}
#audit_alert_dialog .result_block {
    color: #f80c0c;
}
#audit_alert_dialog #audit_alert_dialog_textarea {
    width: 450px;
    height: 90px;
    padding: 5px  10px;
    border: 1px solid #ccc;
    border-radius: 5px;
    resize: none;
    outline: none;
}
#audit_alert_dialog .foot .btn {
    border: 0;
    box-shadow: none;
}
#audit_alert_dialog .foot .btn.cancle {
    background-color: #7dd8ef;
}
#audit_alert_dialog .foot .btn.set {
    background-color: #1ec3ec;
    background-image: url('../../../image/particular/icon_set.png');
}
/*alert_image*/
#alert_image body {
    padding: 0;
}
#alert_image .poster_audit_alert_image {
    width: 500px;
    height: 340px;
    margin: auto;
    background-position: center;
    background-repeat: no-repeat;
    background-size: contain;
}
/*匹配滚动条的样式*/
.mCustomScrollbar .mCustomScrollBox .mCSB_scrollTools {
    width: 10px;
}
.mCustomScrollbar .mCustomScrollBox.mCSB_inside > .mCSB_container {
    margin-right: 15px;
}
.mCustomScrollbar .mCustomScrollBox .mCSB_scrollTools .mCSB_dragger .mCSB_dragger_bar {
    width: 5px;
    border: 1px solid #c5c5c5;
    background-color: #fff;
}
.mCustomScrollbar .mCustomScrollBox .mCSB_scrollTools .mCSB_draggerRail {
    width: 4px;
    background-color: #ececec;
}