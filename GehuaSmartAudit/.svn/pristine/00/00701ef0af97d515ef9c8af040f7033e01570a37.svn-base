/* 海报详情页css */
.poster_audit {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    /*display: none;*/
    background-color: #fff;
    z-index: 99;
    cursor: default;
}
.poster_audit .title_head {
    height: 40px;
    line-height: 40px;
    padding-left: 20px;
    display: flex;
    background-color: #e8eeef;
}
.poster_audit .title_head .title {
    width: 150px;
    height: 40px;
    line-height: 40px;
    text-align: left;
    font-size: 14px;
    color: #434343;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.poster_audit .title_head .middle {
    flex: 1;
}
.poster_audit .title_head .right {
    width: 120px;
    height: 30px;
    line-height: 30px;
    padding-top: 2px;
}
.poster_audit .title_head .right .btn {
    width: 60px;
    height: 24px;
    line-height: 24px;
    border: 1px solid #b4b6b6;
    border-radius: 5px;
    background-color: #efefef;
    color: #787878;
    cursor: pointer;
}
.poster_audit .title_head .right .btn.icon {
    width: 70px;
    height: 30px;
    line-height: 30px;
    padding-left: 30px;
    background-image: url('../../../image/particular/icon_close.png');
    background-size: 14px;
    background-position: 8px center;
    background-repeat: no-repeat;
    font-size: 14px;
}
.poster_audit .container {
    padding: 0 20px 10px;
    background-color: #fff;
}
.poster_audit .container .audit_result, .poster_audit .container .result_detail {
    width: 100%;
    height: 200px;
    margin-top: 30px;
    position: relative;
    border: 1px solid #b7b7b7;
    border-radius: 5px;
    display: flex;
}
.poster_audit .audit_result .title, .poster_audit .result_detail .title {
    width: 100px;
    height: 24px;
    line-height: 24px;
    padding: 0 10px;
    position: absolute;
    top: -13px;
    left: 30px;
    text-align: center;
    background-color: #fff;
    font-size: 14px;
    color: #434343;
}
.poster_audit .audit_result .info {
    padding-top: 30px;
    margin-left: 270px;
    flex: 1;
    display: flex;
    font-size: 12px;
}
.poster_audit .audit_result .info .info_message {
    flex: 1;
}
.poster_audit .audit_result .info_message .content {
    width: 100%;
    height: 30px;
    line-height: 30px;
    display: flex;
}
.poster_audit .audit_result .info_message .content .label {
    width: 60px;
    height: 30px;
    line-height: 30px;
    display: inline-block;
    color: #666;
}
.poster_audit .audit_result .info_message .message .label {
    vertical-align: top;
    position: relative;
    top: 20px;
}
.poster_audit .audit_result .info_message .content .text {
    padding: 0 10px;
    border: 1px solid #ccc;
    border-radius: 5px;
    background-color: #f5f5f5;
    display: inline-block;
}
.poster_audit .audit_result .info_message .content .status {
    width: 240px;
    height: 30px;
    line-height: 30px;
}
.poster_audit .audit_result .info_message .content .pass {
    color: #03d40d;
}
.poster_audit .audit_result .info_message .content .block {
    color: #f80c0c;
}
.poster_audit .audit_result .info_message .content .review {
    color: #777;
}
.poster_audit .audit_result .info_message .content .description {
    /*width: 89%;*/
    flex: 1;
    height: 100px;
    margin-top: 20px;
    margin-right: 20px;
    color: #777;
}
.poster_audit .audit_result .info .btn {
    width: 100px;
    padding-top: 65px;
    display: none;
}
.poster_audit .audit_result .info .btn .pass, .poster_audit .audit_result .info .btn .block {
    width: 40px;
    height: 35px;
    line-height: 35px;
    padding-left: 40px;
    border-radius: 5px;
    color: #fff;
    font-size: 14px;
}
.poster_audit .audit_result .info .btn .pass {
    background-color: #1de10e;
    background-image: url('../../../image/particular/icon_audit_pass.png');
}
.poster_audit .audit_result .info .btn .block {
    margin-top: 15px;
    background-color: #f66755;
    background-image: url('../../../image/particular/icon_audit_block.png');
}
.poster_audit .audit_result .info .btn .icon {
    background-position: 8px center;
    background-repeat: no-repeat;
    cursor: pointer;
}
.poster_audit .result_detail .content {
    padding: 25px 0;
}
.poster_audit .result_detail .content .list_table{
    margin-left: 20px;
}
.poster_audit .result_detail .content #detail_table .detail_pass {
    height: 36px;
    background-position: center;
    background-repeat: no-repeat;
    background-image: url('../../../image/particular/icon_pass.png');
}
.poster_audit .result_detail .content #detail_table .detail_block {
    height: 36px;
    background-position: center;
    background-repeat: no-repeat;
    background-image: url('../../../image/particular/icon_block.png');
}
.poster_audit .result_detail .content #detail_table .detail_review {
    height: 36px;
    background-position: center;
    background-repeat: no-repeat;
    background-image: url('../../../image/particular/icon_review.png');
}
.poster_audit .poster_audit_content {
    margin-top: 15px;
    border: 1px solid #b7b7b7;
    border-radius: 5px;
    overflow: hidden;
}
.poster_audit .poster_audit_content .header {
    height: 44px;
    padding: 0 10px;
    background-color: #fbfbfb;
    display: flex;
}
.poster_audit .poster_audit_content .header .select {
    height: 30px;
    line-height: 30px;
    margin-top: 6px;
    color: #777;
}
.poster_audit .poster_audit_content .header .select {
    border-radius: 20px;
}
.poster_audit .poster_audit_content #recognition_select .select-header {
    border-radius: 15px;
}
.poster_audit .poster_audit_content #recognition_select .select-box ul {
    border-radius: 20px;
}
.poster_audit .poster_audit_content #recognition_select .select-header, .poster_audit .poster_audit_content #recognition_select .select-content li {
    padding: 0 10px;
}
.poster_audit .poster_audit_content #recognition_select .select-header .select-header-content {
    font-size: 12px;
    color: #777;
}
.poster_audit .poster_audit_content .header .middle {
    flex: 1;
}
.poster_audit .poster_audit_content .header .btn {
    width: 130px;
    height: 28px;
    line-height: 28px;
    margin-top: 6px;
    display: flex;
}
.poster_audit .poster_audit_content .header .btn .icon {
    width: 30px;
    height: 30px;
    border: 1px solid #eaeaea;
    border-radius: 5px;
    margin: 0 5px;
    background-color: #fff;
    cursor: pointer;
}
.poster_audit .poster_audit_content .header .btn .poster {
    background-image: url('../../../image/particular/icon_poster.png');
}
.poster_audit .poster_audit_content .header .btn .poster_checked {
    background-color: #fe931f;
    background-image: url('../../../image/particular/icon_poster_checked.png');
}
.poster_audit .poster_audit_content .header .btn .list {
    background-image: url('../../../image/particular/icon_list.png');
}
.poster_audit .poster_audit_content .header .btn .list_checked {
    background-color: #fe931f;
    background-image: url('../../../image/particular/icon_list_checked.png');
}
.poster_audit .poster_audit_content .header .btn .list_margin {
    margin-left: 47px;
}
.poster_audit .poster_audit_content .header .btn .json {
    background-image: url('../../../image/particular/icon_json.png');
}
.poster_audit .poster_audit_content .header .btn .json_checked {
    background-color: #fe931f;
    background-image: url('../../../image/particular/icon_json_checked.png');
}
.poster_audit .poster_audit_content .header .btn .icon {
    background-size: 20px;
    background-position: center;
    background-repeat: no-repeat;
}
.poster_audit .poster_audit_content .content {
    padding: 15px 0 35px 15px;
}
.poster_audit .poster_audit_content .content .content_poster {
    display: flex;
}
.poster_audit .poster_audit_content .content .content_poster, .poster_audit .poster_audit_content .content .content_list, .poster_audit .poster_audit_content .content .content_json {
    display: none;
}
.poster_audit .poster_audit_content .content_poster .poster_left {
    height: 610px;
    padding-bottom: 5px;
    /*flex: 1;*/
    flex: 11;
    border: 1px solid #ccc;
    background-color: #f5f5f5;
}
.poster_audit .poster_audit_content #poster_audit_poster_content .row {
    display: flex;
}
.poster_audit .poster_audit_content #poster_audit_poster_content .col {
    height: 140px;
    flex: 1;
    margin: 10px 10px 1px 10px;
    border: 1px solid #c9c9c9;
    background-color: #eee;
    box-sizing: border-box;
    cursor: pointer;
    position: relative;
}
.poster_audit .poster_audit_content #poster_audit_poster_content .col_checked {
    margin: 9px 9px 0 9px;
    border: 2px solid #00eaff;
    box-shadow: 3px 4px 5px 1px #c9c9c9;
}
.poster_audit .poster_audit_content #poster_audit_poster_content .col .background_img {
    height: 136px;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    background-size: contain;
    background-position: center;
}
.poster_audit .poster_audit_content #poster_audit_poster_content .col .background_time {
    width: 100%;
    position: absolute;
    top: 62px;
}
.poster_audit .poster_audit_content #poster_audit_poster_content .col .creat_time {
    width: 40px;
    height: 15px;
    margin: 0 auto;
    line-height: 15px;
    padding: 2px 10px;
    background: rgba(27, 27, 27, 0.3);
    color: #fff;
}
.poster_audit .poster_audit_content .content_poster .poster_right {
    /*width: 784px;*/
    flex: 14;
    margin-left: 10px;
}
.poster_audit .poster_audit_content .content_poster .poster_right .right_img {
    border: 1px solid #ccc;
    background-color: #eee;
}
.poster_audit .poster_audit_content .content_poster .poster_right .poster_right_img_content {
    height: 440px;
    position: relative;
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center;
}
.poster_audit .poster_audit_content .content_poster .poster_right .right_list {
    height: 165px;
    margin-top: 10px;
}
.poster_audit .poster_audit_content .content_list {
    height: 610px;
    font-size: 12px;
}
.poster_audit .poster_audit_content .content_json {
    height: 610px;
    font-size: 14px;
}

.poster_audit .poster_audit_content .content_json pre {
    padding: 5px;
    margin: 5px;
}


/* 视频标签表格头部样式 */
.poster_audit .poster_audit_content .content_list .tag_table_head {
    display: none;
}
.poster_audit .poster_audit_content .content_list .tag_table_head table {
    width: 100%;
    table-layout: fixed;
    empty-cells: show;
    border-collapse: collapse;
}
.poster_audit .poster_audit_content .content_list .tag_table_head table tr{
    width: 100%;
    display: flex;
}
.poster_audit .poster_audit_content .content_list .tag_table_head table tr:nth-child(1) td{
    background-color: #f8f8f8;
}
.poster_audit .poster_audit_content .content_list .tag_table_head table tr:nth-child(2) td{
    background-color: #78dcf5;
    color: #fff;
}
.poster_audit .poster_audit_content .content_list .tag_table_head table td:nth-child(1),
.poster_audit .poster_audit_content .content_list .tag_table_head table td:nth-child(2),
.poster_audit .poster_audit_content .content_list .tag_table_head table td:nth-child(3),
.poster_audit .poster_audit_content .content_list .tag_table_head table td:nth-child(4),
.poster_audit .poster_audit_content .content_list .tag_table_head table td:nth-child(5) {
    flex: 3;
}
.poster_audit .poster_audit_content .content_list .tag_table_head table td:nth-child(6),
.poster_audit .poster_audit_content .content_list .tag_table_head table td:nth-child(7),
.poster_audit .poster_audit_content .content_list .tag_table_head table tr:nth-child(2) td:nth-child(5),
.poster_audit .poster_audit_content .content_list .tag_table_head table tr:nth-child(2) td:nth-child(6)  {
    flex: 5;
}
.poster_audit .poster_audit_content .content_list .tag_table_head table tr:nth-child(2) td:nth-child(1) {
    flex: 6;
    /* 抵消缺少的border */
    padding-right: 1px;
}
.poster_audit .poster_audit_content .content_list .tag_table_head table td {
    border-top: 1px solid #efefef;
    border-left: 1px solid #efefef;
}
.poster_audit .poster_audit_content .content_list .tag_table_head table tr:nth-child(1) td:nth-child(7),
.poster_audit .poster_audit_content .content_list .tag_table_head table tr:nth-child(2) td:nth-child(6) {
    border-right: 1px solid #efefef;
}
.poster_audit .poster_audit_content .content_list .tag_table_head table td {
    height: 34px;
    line-height: 34px;
    overflow: hidden;
    text-overflow: ellipsis;
    text-align: center;
    font-size: 12px;
    color: #666666;
    font-weight: bold;
}
