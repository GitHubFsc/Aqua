<div class="dialog" id='audit_alert_dialog'>
  <div class="title">
    <div class="label icon" id="audit_label"></div>
    <div class="close popup_dialog_clear"></div>
  </div>
  <div class="body">
    <div class="row">
      <div class="col"><label class='label' for="">{{PARTICULAR_DIALOG_AUDIT_RESULT}}</label><div id="audit_alert_dialog_result" class='result'></div></div>
    </div>
    <div class="row textarea">
      <div class="col"><label class='label' for="">{{PARTICULAR_DIALOG_AUDIT_DESCRIPTION}}</label><textarea id="audit_alert_dialog_textarea"></textarea></div>
    </div>
  </div>
  <div class="foot">
    <div class="btn cancel icon popup_dialog_clear">{{PARTICULAR_DIALOG_CANCEL}}</div>
    <div class="btn set icon">{{PARTICULAR_DIALOG_SET}}</div>
  </div>
</div>