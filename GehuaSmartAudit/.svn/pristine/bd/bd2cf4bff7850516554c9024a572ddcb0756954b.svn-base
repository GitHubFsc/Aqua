<html>
 <head>
 </head>
 <body>
  <div class="popup_dialog_v2"> 
   <div class="popup_dialog_v2_header"> 
    <div class="popup_dialog_v2_header_left"> 
     <div class="popup_dialog_v2_header_left_bar"></div> 
     <div id="popup_add_edit_user_title" class="popup_dialog_v2_header_left_title">
      
     </div> 
    </div> 
    <div class="popup_dialog_v2_close popup_dialog_clear"></div> 
   </div> 
   <div class="popup_dialog_v2_body" style="left:32px;right:30px"> 
		<div style="width:100%;height:44px;margin-top:28px">
			<div class="popup_dialog_v2_input_container" style="margin-top:0;width:280px;float:left"> 
			 <div class="popup_dialog_v2_input_lable" style="width:51px;text-align:right">
				{{USER_ADMIN_USERNAME}}
			 </div> 
			 <input id="popup_add_edit_user_username" class="popup_dialog_v2_input_box" style="height:32px;line-height:32px;font-size:12px;margin-left:25px;width:185px;color:#777777" value="" /> 
			</div> 
			<div class="popup_dialog_v2_input_container" style="margin-top:0;width:280px;float:right"> 
			 <div class="popup_dialog_v2_input_lable" style="width:51px;text-align:right">
				{{USER_ADMIN_NICKNAME}}
			 </div> 
			 <input id="popup_add_edit_user_nickname" class="popup_dialog_v2_input_box" style="color:#777777;height:32px;line-height:32px;font-size:12px;width:185px;margin-left:25px" value="" /> 
			</div> 
		</div>
		<div style="width:100%;height:44px;margin-top:20px">
			<div class="popup_dialog_v2_input_container" style="margin-top:0;width:280px;float:left"> 
			 <div class="popup_dialog_v2_input_lable" style="width:51px;text-align:right">
				{{USER_ADMIN_CONTACT_METHOD}}
			 </div> 
			 <input id="popup_add_edit_user_contact_method" class="popup_dialog_v2_input_box" style="color:#777777;height:32px;line-height:32px;font-size:12px;width:185px;margin-left:25px" value="" /> 
			</div>
			<div class="popup_dialog_v2_input_container" style="margin-top:0;width:280px;float:right"> 
			 <div class="popup_dialog_v2_input_lable" style="width:51px;">
				<div style="float:left">{{USER_ADMIN_ALPHABET_MAIL_PARTA}}</div>
				<div style="float:right">{{USER_ADMIN_ALPHABET_MAIL_PARTB}}</div>
			 </div> 
			 <input id="popup_add_edit_user_mail" class="popup_dialog_v2_input_box" style="color:#777777;height:32px;line-height:32px;font-size:12px;width:185;margin-left:25px;" value="" /> 
			</div> 
		</div> 
   </div> 
   <div class="popup_dialog_v2_foot"> 
    <div id="popup_add_edit_user_operation_button" class="popup_dialog_v2_foot_button_light" style="margin-right:24px">
     <div id="popup_add_edit_user_operation_button_text" class="popup_dialog_v2_foot_content">
      
     </div>
    </div>
		<div id="popup_add_edit_user_cancel_button" class="popup_dialog_v2_foot_button_dark" style="margin-right:18px;background-image:url('uiWidget/images/button_icon_cancel.png');">
     <div class="popup_dialog_v2_foot_content">
      {{USER_ADMIN_CANCEL}}
     </div>
    </div> 		
   </div> 
  </div> 
 </body>
</html>
