}
.menuVideoAudit_content .menuVideoAudit_header .header_options .left .option .option_title {
    text-align: left;
    width: 75px;
    height: 32px;
    line-height: 32px;
    font-size: 12px;
    display: inline-block;
    vertical-align: top; 
}
