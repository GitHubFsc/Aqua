.page_body_container{
	position:absolute;
	top:13px;
	left:13px;
	right:13px;
	bottom:18px;
}
.page_body_title{
	width:100%;
	height:52px;
	background-color:#fbfbfb
}
.page_body_seach_container{
	 border:1px #d9d9d9 solid;
	 border-radius:6px;
	 margin-left:34px;
	 height:30px;
	 width:298px
}
.page_body_seach_container > input{
	height:30px;
	line-height:30px;
	float:left;
	margin-left:15px;
	width:250px;
	border:0;
	color:#777777
}
.page_body_seach_icon{
	width:23px;
	height:18px;
	cursor:pointer;
	border-left:1px #e2e2e2 solid;
	background: url(../image/common/search_icon.png) no-repeat;
	background-position:right center
}
.page_body_calendar_container{
	color:#777777;
	width:133px;
	height:32px
}
.page_body_calendar_middle_line_container{
	height:100%;
	width:18px;
	position:relative
}
.page_body_calendar_middle_line{
	height:1px;
	width:10px;
	background-color:#9d9d9d;
	position:absolute;
	left:50%;
	top:50%;
	margin-left:-5px
}
.page_body_content_container{
	position:relative;
	width:100%;
	height:calc(100% - 52px);
	background-color:#ffffff;
	border:1px #dfdfdf solid;
}
.page_body_table_container{
	position:absolute;
	left:14px;
	top:13px;
	right:13px;
	bottom:0px;
	border:1px #dfdfdf solid
}
.page_body_table_operation_button{
	color:#40b8d8;
	margin-left: 12px;
  margin-right: 12px;
	cursor:pointer
}

.iot_list_operate {
	cursor: default;
	margin-right: 25px;
}

.iot_list_operate:last-child {
	margin-right: 0;
}

.operate_active {
	cursor: pointer;
	color: #40b8d8;
}
.confirm_text_container{
	text-align:center;
	width:100%;
	position:absolute;
	text-align:center;
	color:#434343;
	height:30px;
	line-height:30px;
	font-size:18px;
	top:79px
}
.checkbox_text_container{
	cursor:pointer;
	color:#828282;
	font-size:12px;
	overflow:hidden;
	float:left
}
.checkbox_text_checkbox{
	border: 1px solid #d6d6d6;
  border-radius: 4px;
  float: left;
  height: 14px;
  width: 14px;
}
.checkbox_text_checkbox_son_light,.checkbox_text_checkbox_son_dark{
	width:10px;
	height:10px;
	border-radius:2px;
	margin:2px
}
.checkbox_text_checkbox_son_dark{
	background-color:#d4d4d4
}
.checkbox_text_checkbox_son_light{
	background-color:#32beea
}
.checkbox_text_text{
  width:133px;
	max-width:133px;
	padding-left:12px;
	height:16px;
	line-height:16px;
	white-space:nowrap;
	text-overflow:ellipsis;
	overflow:hidden;
	float:left
}
.checkbox_text_text_style2{
  width:133px;
	padding-left:12px;
	height:16px;
	line-height:16px;
	float:left;
	margin-right:10px;
}
.dashed_line{
	height:1px;
	border-top: 1px #bfbfbf dashed;
	width:calc(50% - 45px);
	margin-top:31px
}
.dashed_line_container{
	width:100%;
	overflow:hidden
}
.dashed_line_container_text{
	height:66px;
	line-height:66px;
	width:90px;
	color:#747474;
	text-align:center
}
.menu_row_container{
	border-bottom:1px #cccccc solid;
	height:30px;
	line-height:30px;
	padding-left:15px;
	width:185px;
	cursor:pointer
}
.styled_selector_inputbox{
	border:none;
	color:rgb(121, 121, 121);
	padding-left:19px;
	max-width:93px;
	height:30px;
	background:transparent;
	overflow:hidden
}
.styled_selector_menu_body{
	border-left:1px solid #dadada;
	border-right:1px solid #dadada;
	max-height:500px;
	position:absolute;
	top:31px;
	width:138px;
	overflow-y:auto;
	overflow-x:hidden;
	z-index:2;
	background-color:#ffffff;
	display:none;
	border-bottom-right-radius:4px;
  border-bottom-left-radius:4px;
}
.styled_selector_menu_body_items{
	border-bottom:1px solid #dadada;
	width:119px;
	height:32px;
	padding-left:19px;
}
.content_body_container,.content_body_container_style2{
	position:absolute;
	top:1px;
	left:1px;
	border:1px #dfdfdf solid;
	bottom:2px;
	height:auto
}