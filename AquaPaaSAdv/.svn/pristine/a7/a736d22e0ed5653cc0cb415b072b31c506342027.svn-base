#lecture_ticket{
    position: relative;
    width: 100%;
    height: 100%;
    background: white;
}

#lecture_ticket input:not([type="image"]), textarea {
    box-sizing: border-box;
}
#lecture_ticket .panel_top_search_box_handle {
    width: 16px;
    height: 16px;
    cursor: pointer;
    background-image: url(../../image/ticket/search_handle.png);
    background-repeat: no-repeat;
    background-position: center;
    position: absolute;
    top: 50%;
    margin-top: -8px;
    right: 11px;
}

#lecture_ticket .panel_page_head {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    height: 60px;
}
#lecture_ticket .panel_page_head_left {
    position: absolute;
    left: 18px;
    height: 36px;
    top: 50%;
    margin-top: -18px;
}
#lecture_ticket .panel_page_head_left > div {
    display: inline-block;
    vertical-align: middle;
}
#lecture_ticket .panel_page_head_title_lead {
    width: 3px;
    height: 22px;
    background-color: rgb(44,167,222);
}
#lecture_ticket .panel_page_head_title {
    height: 36px;
    line-height: 36px;
    color: rgb(73,75,88);
    margin-left: 9px;
    margin-right: 15px;
    font-size: 16px;
    font-weight: bold;
}
#lecture_ticket .panel_page_head_title_follower {
    background-color: rgb(181,183,198);
    width: 1px;
    height: 36px;
}
#lecture_ticket .panel_page_body {
    position: absolute;
    top: 61px;
    left: 0;
    right: 0;
    bottom: 0;
    border-style: solid;
    border-color: rgb(237,237,219);
    border-width: 1px 1px 0 1px;
    background-color: rgb(252,252,242);
}
#lecture_ticket .panel_top_search_box {
    width: 300px;
    height: 36px;
    margin-left: 15px;
    position: relative;
}
#lecture_ticket .panel_top_search_box_input {
    position: absolute;
    width: 298px;
    height: 34px;
    border: 1px solid rgb(234,234,234);
    background-color: rgb(244,244,244);
    color: rgb(121,121,121);
    padding-left: 10px;
    padding-right: 30px;
    font-size: 14px;
}
#lecture_ticket .panel_page_head_right {
    position: absolute;
    right: 8px;
    height: 40px;
    top: 50%;
    margin-top: -20px;
    text-align: right;
}
#lecture_ticket .panel_page_head_right>div {
    vertical-align: middle;
    display: inline-block;
}
#lecture_ticket .panel_page_head_button {
    background-color: rgb(46,162,215);
    color: white;
    height: 38px;
    border: 1px solid rgb(89,127,137);
    line-height: 38px;
    padding: 0 22px;
    font-size: 16px;
    cursor: pointer;
}

.select-box {
    float: left;
    position: relative;
}
.select-box ul,li{
    margin: 0;
    padding: 0;
}
.select-status .select-header {
    width: 88px;
    height: 38px;
    border: 1px solid rgb(198, 198, 198);
    padding-right: 26px;
    background: url(../../image/ticket/arrow.png) no-repeat #f9f9f9;
    background-position: 95% 14px;
    line-height: 38px;
    text-indent: 14px;
    font-size: 14px;
    color: #797979;
    cursor: pointer;
    overflow: hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
    -o-text-overflow:ellipsis;
    overflow: hidden;
}
.select-status .select-header1 {
    width: 88px;
    height: 38px;
    border: 1px solid rgb(198, 198, 198);
    padding-right: 26px;
    background: url(../../../images/lecture/lectureListselect.png) no-repeat #f9f9f9;
    background-position: 95% 6px;
    line-height: 38px;
    text-indent: 14px;
    font-size: 14px;
    color: #a9a9a9;
    cursor: pointer;
    overflow: hidden;
}
.select-status .select-arrow {
    background-position: 95% -9px;
}
.select-status .select-arrow1 {
    background-position: 95% 7px;
}
.select-status .select-content {
    border-left: 1px solid #eaeaea;
    border-right: 1px solid #eaeaea;
    top: 38px;
    left: 0;
    z-index: 99;
    display: none;
    background-color: #f9f9f9;
    position: absolute;
}
.select-status .select-content li {
    list-style: none;
    width: 114px;
    height: 30px;
    margin: 0;
    border-bottom: 1px solid #eaeaea;
    line-height: 38px;
    text-indent: 14px;
    background-color: #f9f9f9;
    font-size: 16px;
    color: #a9a9a9;
    cursor: pointer;
    white-space:nowrap;
    text-overflow:ellipsis;
    -o-text-overflow:ellipsis;
    overflow: hidden;
}

#lecture_ticket_list {
    position: absolute;
    left: 8px;
    right: 8px;
    bottom: 10px;
    top: 0px;
    font-size: 14px;
}
.lecture_ticket_page_head_title_back .header .btn {
    color: #FFFFFF !important;
    border: 1px solid #597F89;
    border-radius: 3px;
    background-color: #2EA2D7;
    cursor: pointer;
}
.lecture_ticket_page_head_title_back .header .back {
    width: 58px;
    padding-left: 48px;
    background-image: url(../../image/ticket/left_arrow.png);
    background-position: 19px center;
    background-repeat: no-repeat;
}
.lecture_ticket_page_head_title_back .header .header_item {
    height: 37px;
    line-height: 37px;
    font-size: 16px;
    font-weight: bold;
    color: #494B58;
}

.lecture_ticket_dialog_label_row{
    float:left;
    width: calc(50% - 35px);
    padding: 10px 10px 0px 25px;
    height:55px;
}
.lecture_ticket_dialog_label_row > div {
    display: block;
    font-size: 14px;
    vertical-align: middle;
}
.lecture_ticket_dialog_label {
    color: rgb(121,121,121);
    line-height: 32px;
    height: 32px;
    margin-right: 20px;
    width: 100%;
}
.lecture_ticket_dialog_label_row .input {
    color: rgb(121,121,121);
    background-color: white;
    border: 1px solid rgb(198,198,198);
    height: 30px;
    width: calc(100% - 20px);
    padding: 0 10px;
    font-size: 14px;
    margin-right: 6px;
}
#lecture_ticket_code_input {
    width: 195px;
}
#lecture_ticket_item_name{
    background-color: rgb(239,239,239);
}
#lecture_ticket_dialog_get_info {
    width: 90px;
    height: 28px;
    border: 1px solid rgb(139,139,139);
    background-color: #2EA3D9;
    cursor: pointer;
    color: white;
    text-align: center;
    line-height: 30px;
    border-radius: 2px;
    display: inline-block;
}
.lecture_ticket_dialog_cancel, .lecture_ticket_dialog_submit {
    width: 149px;
    height: 38px;
    bottom: 23px;
    color: white;
    border: 1px solid rgb(89,127,137);
    cursor: pointer;
    line-height: 38px;
    text-align: center;
    font-size: 16px;
    display: inline-block;
}
.lecture_ticket_dialog_cancel{
    margin-top: 20px;
    background: rgb(94,160,192);
    margin-left: 130px;
}
.lecture_ticket_dialog_submit{
    margin-top: 20px;
    background: rgb(46,161,215);
}

.lecture_ticket_op{
    color: rgb(46,162,215);
    margin: 0 8px;
    cursor: pointer;
}

.lecture_ticket_op.displayed{
    cursor: text;
    color: rgb(121,121,121);
    background: transparent;
    border: 0px;
}

#lecture_ticket_date_input_from{
    color: rgb(121, 121, 121);
    height: 32px;
    width: 322px;
    font-size: 14px;
    text-indent: 13px;
    font-family: '����,Arial';
    border: 1px solid rgb(198, 198, 198);
}
#lecture_ticket_item_type .select-status .select-header{
  width: calc(100% - 26px);
  line-height: 30px;
}
#lecture_ticket_item_type .select-status .select-content{
  width: 100%;
}
#lecture_ticket_item_type .select-status .select-content li{
  width: 100%;
}
#lecture_ticket_search_type .select-status .select-header{
    width: calc(100% - 26px);
    line-height: 30px;
}
#lecture_ticket_search_type .select-status .select-content{
    width: 100%;
}
#lecture_ticket_search_type .select-status .select-content li{
    width: 100%;
}


.manage_book_dialog_del_icon {
    position: absolute;
    left: 20px;
    top: 15px;
    width: 94px;
    height: 103px;
    background-image: url(../../image/ticket/manage_book_dialog_del_icon.png);
}
.manage_book_dialog_del_info {
    position: absolute;
    width: 200px;
    height: 100px;
    left: 130px;
    top: 45px;
    font-size: 16px;
    color: rgb(121,121,121);
}

.lecture_recommend_dialog_cancel, .lecture_recommend_dialog_submit{
    position: absolute;
    width: 149px;
    height: 38px;
    bottom: 23px;
    color: white;
    border: 1px solid rgb(89,127,137);
    cursor: pointer;
    line-height: 38px;
    text-align: center;
    font-size: 16px;
}

.lecture_recommend_dialog_cancel {
    right: 201px;
    background-color: rgb(93,161,192);
}

.lecture_recommend_dialog_submit {
    right: 25px;
    background-color: rgb(46,162,215);
}

#lecture_recommend_del_cancel {
    right: 150px;
    width: 108px;
    height: 38px;
    line-height: 38px;
    bottom: 10px;
}

#lecture_recommend_del_confirm {
    right: 20px;
    width: 108px;
    height: 38px;
    line-height: 38px;
    bottom: 10px;
}