<div id="editLimit" class="editLimit">
  <div class="_dialog_body">
    <div class="_title_bar">
      <div class="popup_dialog_clear">{{OPS_TEXT_BACK}}</div>
      <div class="sucai_name"></div>
      <div class="split"></div>
      <input class="site_searcher" />
    </div>
    <div class="main_body">
      <div class="limit_info">
        <div class="row">
          <div class="hint">{{SUCAI_EDITLIMIT_ENABLE}}</div>
          <div class="radio1" data-value="1"></div>
          <div class="radio1_hint1" style="width: 33px;padding-left: 10px;">{{SUCAI_EDITLIMIT_YES}}</div>
          <div class="radio1" data-value="0"></div>
          <div class="radio1_hint2" style="width: auto;padding-left: 10px;">{{SUCAI_EDITLIMIT_NO}}</div>
          <div class="span"></div>
          <div class="hint">{{SUCAI_EDITLIMIT_TOTAL}}</div>
          <input type="text" id="limit_total_quota">
          <div class="hint">{{SUCAI_EDITLIMIT_PATCH}}</div>
          <input type="text" id="placement_times">
          <div class="span"></div>
          <div class="save_info">{{SUCAI_EDITLIMIT_SAVE}}</div>
        </div>
        <!-- <div class="row">
          <div class="hint">{{SUCAI_EDITLIMIT_TEMINAL_PERIOD}}</div>
          <input type="text" id="teminal_period">
          <div class="hint">{{SUCAI_EDITLIMIT_TEMINAL_PERIOD_COUNT}}</div>
          <input type="text" id="teminal_period_count">
          <div class="save_info">{{SUCAI_EDITLIMIT_SAVE}}</div>
        </div> -->
      </div>
      <div class="list_table">
        <div class="table_title">
          <div class="table_title_text">{{SUCAI_LIMITSITE_INFO}}</div>
          <div class="add_site">{{SUCAI_ADD_SITE}}</div>
        </div>
        <div class="list_table_main" id="list_table_sites"></div>
      </div>
    </div>
  </div>
</div>
