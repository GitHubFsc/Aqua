<div id="adPolicyAuditContainer" class="panel_container">
  <link rel="stylesheet" href="style/adPolicyAudit.css" media="screen" title="no title" charset="utf-8">
  <link rel="stylesheet" href="uiWidget/styles/select.css" media="screen" title="no title" charset="utf-8">
  <div class="adPolicyAuditMenu">
    <div class="policy_type">
      <div name="type" id="policy_img">{{ADPOLICYAUDIT_POLICYTYPE_IMG}}</div>
      <div name="type" id="policy_video">{{ADPOLICYAUDIT_POLICYTYPE_VIDEO}}</div>
      <div name="type" id="policy_txt">{{ADPOLICYAUDIT_POLICYTYPE_SUBTITLE}}</div>
    </div>
    <div class="menu_split">
    </div>
    <div class="policy_status">
      <div name="status" id="policy_firstAudit">{{ADPOLICYAUDIT_POLICYSTATUE_FIRSTAUDIT}}</div>
      <div name="status" id="policy_secondAudit">{{ADPOLICYAUDIT_POLICYSTATUE_THIRDAUDIT}}</div>
      <div name="status" id="policy_enable">{{ADPOLICYAUDIT_POLICYSTATUE_ENABLE}}</div>
      <div name="status" id="policy_disable">{{ADPOLICYAUDIT_POLICYSTATUE_DISABLE}}</div>
    </div>
    <div class="policy_search">
      <input type="text" name="policy_search" id="policy_searchInput" value="">
      <div class="policy_searchBtn" name="policy_search" id="policy_searchBtn"></div>
    </div>
  </div>
  <div class="adPolicyAuditBody">
    <div class="adPolicyAuditTable" id="adPolicyAuditTable"></div>
  </div>
</div>
