.main_menu_tooltip {
	height: 24px;
	line-height: 24px;
	font-size: 12px;
	position: absolute;
	left: 100%;
	margin-left: -5px;
	word-break: keep-all;
	white-space: nowrap;
	top: 8px;
}

.main_menu_tooltip_arrow {
	position: absolute;
	width: 0;
	height: 0;
	top: 8px;
	border-width: 4px 7px;
	border-style: solid;
	border-color: transparent rgb(41,48,56) transparent transparent;
}

.main_menu_tooltip_text {
	position: absolute;
	height: 24px;
	border-radius: 3px;
	padding-left: 5px;
	padding-right: 5px;
	background-color: rgb(41,48,56);
	color: rgb(150,154,176);
	left: 12px;
}

.styled-selector-down-arrow {
	background-image: url(../uiWidget/images/selector_arrow_downward.png);
	background-repeat: no-repeat;
	width: 20px;
	height: 13px;
}

.styled-selector-up-arrow {
	background-image: url(../uiWidget/images/selector_arrow_downward.png);
	background-repeat: no-repeat;
	-webkit-transform: rotate(180deg);
	        transform: rotate(180deg);
	width: 20px;
	height: 13px;
}

.styled-selector-dropicon {
	right: 8px;
}

.loading_layer {
    position: fixed;
    left: 0;
    top: 0;
    z-index: 9999;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.1);
    background-image: url("../image/index/loading.gif");
    background-position: center;
    background-repeat: no-repeat;
}

.main_page_user_container {
	position: absolute;
	right: 40px;
	width: 120px;
	height: 55px;
	top: 0;
}

.main_page_user_icon {
	position: absolute;
	width: 36px;
	height: 36px;
	border-radius: 18px;
	top: 50%;
	margin-top: -18px;
	left: 0;
	background-position: center;
	background-repeat: no-repeat;
}

.main_page_user_info {
	position: absolute;
	height: 36px;
	top: 50%;
	margin-top: -18px;
	left: 45px;
	right: 0;
	cursor: default;
}

.main_page_user_info_name {
	width: 100%;
	height: 20px;
	line-height: 20px;
	font-size: 12px;
	color: white;
	white-space: nowrap;
	word-break: keep-all;
	text-overflow: ellipsis;
	overflow: hidden;
}

.main_page_user_info_role {
	width: 100%;
	height: 16px;
	line-height: 16px;
	font-size: 10px;
	color: rgb(148,149,150);
	white-space: nowrap;
	word-break: keep-all;
	text-overflow: ellipsis;
	overflow: hidden;
}

.main_page_user_dropdown {
	position: absolute;
	height: 55px;
	top: 0;
	right: 22px;
	width: 14px;
}

.main_page_user_dropdown_icon {
	background-image: url(../image/index/user_drop_down_icon.png);
	background-repeat: no-repeat;
	background-position: center;
	position: absolute;
	top: 0;
	width: 14px;
	height: 100%;
	cursor: pointer;
	-webkit-transform: rotate(0deg);
	        transform: rotate(0deg);
}

.main_page_user_dropdown_down .main_page_user_dropdown_icon {
}

.main_page_user_dropdown_block {
	position: absolute;
	top: 50%;
	margin-top: 7px;
	width: 65px;
	left: 50%;
	margin-left: -40px;
	display: none;
}

.main_page_user_dropdown_down .main_page_user_dropdown_block {
	display: block;
}

.main_page_user_dropdown_block_arrow {
	position: absolute;
	right: 10px;
	width: 0;
	height: 0;
	border-width: 5px 3px;
	border-style: solid;
	border-color: transparent transparent rgb(69,71,85) transparent;
}

.main_page_user_dropdown_block_content {
	position: absolute;
	top: 3px;
	right: 0;
	width: 100%;
	border-radius: 3px;
	background-color: rgb(64,66,80);
	border: 1px solid rgb(86,88,107);
}

.main_page_user_dropdown_block_item {
	height: 24px;
	line-height: 24px;
	font-size: 13px;
	width: calc(100% - 5px);
	text-align: right;
	color: rgb(129,135,179);
	cursor: pointer;
	padding-right: 5px;
}

#main_page_user_platforms {
	position: absolute;
	top: 14px;
	height: 25px;
	right: 180px;
	width: 80px;
	background-color: #293038;
	color: #fff;
}

#main_page_user_platforms .styled-selector-inputbox{
	background-color: transparent;
	border: none;
	color: #fff;
	padding-left: 10px;
	font-size: 20px;
	font-family: "宋体";
}

#main_page_user_platforms .styled-selector-dropbox td{
	padding-left: 10px;
}

#main_page_user_platforms .styled-selector-dropbox td:hover{
	background-color: rgb(0,153,203);
}

#main_page_user_platforms > select {
    position: absolute;
    height: 22px;
    top: 18px;
    right: 0;
}
