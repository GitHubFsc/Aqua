html, body {
	padding: 0;
	margin: 0;
	width: 100%;
	height: 100%;
	min-height: 720px;
	min-width: 1280px;
}

body {
	font-size: 14px;
	font-family: 宋体;
}

*{
	margin:0;
	padding:0;
}

.login_page {
	position: relative;
	width: 100%;
	height: 100%;
}

.login_head {
	position:relative;
	height: 60px;
	width: 100%;
	background-color: rgb(63,63,63);
	z-index: 1;
}

.login_body {
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	top: 55px;
	border-radius: 5px 5px 0 0;
	background-color: rgb(77,82,91);
	z-index: 10;
}

.login_head_icon {
	position: absolute;
	left: 15px;
	top: 12px;
	width: 120px;
	height: 30px;
	background-image: url(../image/index/login_head_icon_bg.png);
}

.login_dialog {
	position: absolute;
	left: 50%;
	margin-left: -238px;
	width: 475px;
	height: 474px;
	background-image: url(../image/index/login_dialog_bg.png);
	background-repeat: no-repeat;
	top: 105px;
}

.login_dialog_icon {
	background-image: url(../image/index/login_dialog_icon_bg.png);
	background-repeat: no-repeat;
	width: 74px;
	height: 97px;
	position: absolute;
	left: 50%;
	margin-left: -37px;
	top: -77px;
}

.login_dialog_title {
	position: absolute;
	left: 50%;
	margin-left: -193px;
	width: 386px;
	height: 60px;
	top: 39px;
}

.login_dialog_title_icon {
	background-image: url(../image/index/login_dialog_title_icon_bg.png);
	background-position: center;
	background-repeat: no-repeat;
	background-size: contain;
	position: absolute;
	width: 96px;
	height: 60px;
	left: 25px;
}

.login_dialog_title_text {
	line-height: 60px;
	height: 60px;
	text-align: center;
	position: absolute;
	left: 140px;
	font-size: 38px;
	color: rgb(100,100,100);
	font-weight: bold;
}

.login_dialog_inputs {
	position: absolute;
	left: 50%;
	margin-left: -193px;
	width: 386px;
	top: 114px;
	height: 170px;
	background-image: url(../image/index/login_dialog_inputs_bg.png);
	background-repeat: no-repeat;
	background-position: center;
}

.login_dialog_inputs input {
	position: absolute;
	border: none;
	color: rgb(189,189,189);
	height: 84px;
	line-height: 84px;
	font-size: 20px;
	left: 70px;
	width: 300px;
	background-color: transparent;
	color: rgb(109,109,109);
}

.login_dialog_user_mail, .login_dialog_user_pwd {
	position: relative;
	width: 100%;
	height: 84px;
}

.login_dialog_user_mail_icon {
	position: absolute;
	height: 88px;
	left: 15px;
	width: 44px;
	background-image: url(../image/index/login_dialog_input_user_mail_icon_bg.png);
	background-repeat: no-repeat;
	background-position: center;
}

.login_dialog_user_pwd_icon {
	position: absolute;
	height: 88px;
	left: 15px;
	width: 44px;
	background-image: url(../image/index/login_dialog_input_user_pwd_icon_bg.png);
	background-repeat: no-repeat;
	background-position: center;
}

.login_dialog_login_button {
	background-image: url(../image/index/login_dialog_login_button_bg.png);
	background-position: center;
	background-size: contain;
	color: white;
	text-align: center;
	width: 386px;
	height: 76px;
	position: absolute;
	left: 50%;
	margin-left: -193px;
	line-height: 76px;
	font-size: 32px;
	top: 334px;
	font-weight: bold;
	cursor: pointer;
}

.login_dialog_register_option {
	position: absolute;
	width: 386px;
	height: 20px;
	left: 50%;
	margin-left: -193px;
	font-size: 20px;
	line-height: 20px;
	text-align: right;
	position: absolute;
	top: 420px;
}

.login_dialog_register_ask {
	color: rgb(200,200,200);
}

.login_dialog_register_link {
	color: rgb(74,150,203);
	cursor: pointer;
}

.login_dialog_rmb_user_block {
	position: absolute;
	top: 284px;
	left: 50%;
	margin-left: -193px;
	width: 386px;
	height: 50px;
	line-height: 50px;
	font-size: 18px;
	color: rgb(189,189,189);
}

.login_dialog_rmb_user_block > div {
	position: absolute;
	height: 100%;
	top: 0;
}

.login_dialog_rmb_user_block > div:nth-child(1) {
	left: 12px;
	text-align: left;
}

.login_dialog_rmb_user_block > div:nth-child(2) {
	right: 12px;
	text-align: right;
}

.login_dialog_checkbox {
	display: inline-block;
	background-color: white;
	border: 1px solid rgb(195,197,199);
	width: 16px;
	height: 16px;
	vertical-align: middle;
}

.login_dialog_checkbox > div {
	width: 14px;
	height: 14px;
	position: relative;
	left: 1px;
	top: 1px;
	background-color: rgb(56,135,196);
	display: none;
}

div.login_dialog_checkbox.login_dialog_checkbox_checked > div {
	display: block;
}

.main_page {
	position: relative;
	width: 100%;
	height: 100%;
}

.main_left_block {
	position: relative;
	width: 55px;
	min-height: 100%;
	background-color: rgb(41,48,56);
	z-index: 10;
}

.main_expand .main_left_block {
	width: 125px;
}

.main_right_block {
	position: absolute;
	top: 0;
	bottom: 0;
	left: 55px;
	right: 0;
	z-index: 1;
}

.main_expand .main_right_block {
	left: 125px;
}

.main_expandable_front {
	width: 55px;
	height: 100%;
	position: relative;
	display: inline-block;
	vertical-align: top;
	background-position: center;
	background-repeat: no-repeat;
}

.main_expandable_back {
	height: 100%;
	width: 60px;
	display: none;
	position: relative;
	overflow: hidden;
	vertical-align: top;
	text-overflow: ellipsis;
	word-break: keep-all;
	white-space: nowrap;
}

.main_expand .main_expandable_back {
	display: inline-block;
}

.main_page_icon {
	height: 55px;
	background-color: rgb(68,91,102);
}

.xor_logo_front {
	background-image: url(../image/index/xor_logo_f.png);
}

.main_expand .xor_logo_front{
	background-position: right center;
}

.xor_logo_back {
	background-image: url(../image/index/xor_logo_b.png);
	background-repeat: no-repeat;
	background-position: left 27px;
}

.main_page_menu {
	position: relative;
	width: 100%;
	/*top: 76px;
	bottom: 0;*/
}

.main_page_head_bg {
	position: relative;
	height: 59px;
	width: 100%;
	background-color: rgb(41,48,56);
	z-index: 1;
}

.main_page_head {
	position: absolute;
	top: 0;
	height: 55px;
	width: 100%;
	z-index: 30;
}

.main_page_content_title {
	position: absolute;
	left: 14px;
	line-height: 55px;
	height: 55px;
	color: white;
	font-size: 24px;
}

.content_container {
	position: absolute;
	left: 0;
	right: 0;
	top: 55px;
	bottom: 0;
	border-radius: 4px 0 0 0;
	z-index: 10;
	background-color: white;
}

.main_menu_toggle_button {
	background-image: url(../image/index/main_page_menu_toggle.png);
	background-position: center 4px;
	background-repeat: no-repeat;
	width: 100%;
	background-color: rgb(35,40,46);
	height: 20px;
	cursor: pointer;
	border-style: solid;
	border-color: rgb(72,83,96);
	border-width: 0 0 1px 0;
}

.main_expand .main_menu_toggle_button {
	background-image: url(../image/index/main_page_menu_toggle_expand.png);
	background-position: center center;
}

.main_page_menu > div {
	border-style: solid;
	border-width: 0 0 1px 0;
	border-color: rgb(72,83,96);
	cursor: pointer;
	color: rgb(150,154,176);
	font-size: 14px;
	font-weight: bold;
	height: 40px;
	line-height: 40px;
	position: relative;
}

.main_page_menu > div.main_menu_group {
	height: auto;
}

.main_page_menu > div:last-child {
	border-style: none;
}

.main_menu_group > div {
	border-style: solid;
	border-width: 0 0 1px 0;
	border-color: rgb(72,83,96);
	cursor: pointer;
	color: rgb(150,154,176);
	font-size: 14px;
	font-weight: bold;
	height: 40px;
	line-height: 40px;
	position: relative;
}

.main_menu_group.main_menu_group_shrink > div {
	display: none;
}

.main_menu_group.main_menu_group_shrink > div:first-child {
	display: block;
	border-style: none;
}

.main_menu_group > div:last-child {
	border-style: none;
}

.main_menu_group_title {
	background-color: rgb(35,40,46);
}

.main_menu_group_title_front {
	background-image: url(../image/index/main_menu_group_expand.png);
	-webkit-transform: rotate(0deg);
	        transform: rotate(0deg);
}

.main_menu_group_shrink .main_menu_group_title_front {
	-webkit-transform: rotate(-90deg);
	        transform: rotate(-90deg);
}

.main_page_menu > div.main_menu_focus, .main_menu_group > div.main_menu_focus {
	background-color: rgb(0,153,203);
	color: white;
}

