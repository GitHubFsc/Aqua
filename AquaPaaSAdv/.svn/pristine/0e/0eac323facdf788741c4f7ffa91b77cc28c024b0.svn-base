/***************** head ***************/

.sucai_head {
    padding: 8px 0;
    border: 1px solid white;
    padding: 8px 0;
    position: relative;
    height: 40px;
    top: 2px;
}

.sucai_head_left {
    float: left;
    width: 80%;
}

.sucai_head_right {
    float: right;
    margin-right: 12px;
}

.sucai_add_class_btn {
    border-radius: 2px;
    height: 38px;
    line-height: 38px;
    background: #8fc31f;
    color: white;
    padding-left: 10px;
    padding-right: 20px;
    cursor: pointer;
    position: relative;
}
.sucai_add_class_btn .select_header {
    height: 38px;
    line-height: 36px;
    font-size: 0px;
    line-height: 0px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}
.sucai_add_class_btn .select_header .select_header_add_icon {
    vertical-align: middle;
    height: 36px;
    line-height: 36px;
}
.sucai_add_class_btn .select_header .select_header_content {
    font-size: 14px;
    font-weight: bold;
    height: 36px;
    line-height: 36px;
    vertical-align: middle;
    margin-right: 12px;
}
.sucai_add_class_btn .select_header .select_header_open_icon {
    line-height: 36px;
    vertical-align: middle;
}
.sucai_add_class_btn .select-content {
    border-top: 1px solid rgb(234, 234, 234);
    border-left: 1px solid rgb(234, 234, 234);
    border-right: 1px solid rgb(234, 234, 234);
    margin: 0px;
    padding: 0px;
    right: 0px;
    z-index: 99;
    background-color: rgb(255, 255, 255);
    position: absolute;
    box-sizing: border-box;
    width: 182px;
    top: 36px;
    display: none;
}
.sucai_add_class_btn .select-content li {
    list-style: none;
    margin: 0px;
    border-bottom: 1px solid rgb(234, 234, 234);
    text-indent: 10px;
    background-color: rgb(255, 255, 255);
    font-size: 14px;
    color: #727272;
    cursor: pointer;
    box-sizing: border-box;
    text-align: left;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 100%;
    height: 36px;
    line-height: 36px;
    text-align: right;
    padding-right: 10px;
}
.sucai_add_class_btn .select-content li:hover{
    color: white;
    background:#6DBBE1
}
.sucai_add_class_btn.opened .select-content{
    display: block;
}
.sucai_add_class_btn.opened .select_header_open_icon{
    transform: rotate(180deg)
}


.sucai_add_class_btn.page_button_inactive {
    cursor: default;
    opacity: 0.4;
}

.sucai_head_center {
    zoom: 1;
    height: 36px;
    margin-left: 10px;
    border-right: 1px solid #b5b7c6;
    padding-right: 10px;
    margin-right: 10px;
    float: left;
}

.sucai_head_center li {
    cursor: pointer;
    list-style: none;
    float: left;
    width: 87px;
    height: 36px;
    text-align: center;
    line-height: 36px;
    border: 1px solid #e2e2e2;
    color: black;
    font-weight: bold;
    font-size: 16px;
    font-family: 宋体;
    border-radius: 2px;
    background: -webkit-gradient(linear, left top, left bottom, from(#f7f7f7), to(#e4e4e4));
    background: linear-gradient(#f7f7f7, #e4e4e4);
    background: -ms-linear-gradient(#f7f7f7, #e4e4e4);
}

.sucai_head_center li.sucai_menu_selected {
    color: white;
    font-weight: bold;
    background: #75b8d6;
    border: 1px solid #4488a7;

    border-right: none;
}

.sucai_content_container {
    margin-left: 7px;
    margin-right: 7px;
    border: 1px solid #ededdb;
    padding: 2px;
    background-color: #fcfcf2;
}

.sucai-site-filter {
    width: 131px;
    height: 36px;
    z-index: 800;
    float: left;
    position: relative;
    top: 1px;
    margin-right: 20px;
}

