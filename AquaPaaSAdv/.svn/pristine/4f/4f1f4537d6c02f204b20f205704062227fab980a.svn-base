<style>
	.lecture_recommend_dialog.wp_panel_dialog {
		position: absolute;
		left: 0;
		top: 0;
		right: 0;
		bottom: 0;
		border: 1px solid #177ba9;
		font-size: 14px;
	}
	.lecture_recommend_dialog.wp_panel_dialog  input:not([type="image"]), textarea {
		box-sizing: border-box;
	}

	.lecture_recommend_dialog.wp_panel_dialog .wp_panel_dialog_title {
		position: absolute;
		left: 0;
		right: 0;
		top: 0;
		height: 40px;
		background-color: #2ea2d7;
	}

	.lecture_recommend_dialog.wp_panel_dialog .wp_panel_dialog_title_label {
		position: absolute;
		height: 40px;
		line-height: 40px;
		top: 0;
		left: 20px;
		font-size: 19px;
		color: white;
	}

	.lecture_recommend_dialog.wp_panel_dialog .wp_panel_dialog_content {
		position: absolute;
		left: 0;
		right: 0;
		bottom: 0;
		top: 40px;
		background-color: white;
	}

	.lecture_recommend_dialog.wp_panel_dialog .wp_panel_dialog_close {
		position: absolute;
		width: 38px;
		height: 38px;
		right: 15px;
		background-image: url('./image/ticket/manage_dialog_close.png');
		background-position: center;
		background-repeat: no-repeat;
		cursor: pointer;
	}

	.lecture_recommend_dialog.wp_panel_dialog .wp_panel_dialog_cancel, .wp_panel_dialog_submit {
		position: absolute;
		border: 1px solid #597f89;
		bottom: 10px;
