			{{LECTURETICKET_NINSHIFUQUEDINGYAOLIJISHIXIAOGAIPIKECHENGQUAN}}
		</div>
		<div class="lecture_recommend_dialog_cancel popup_dialog_clear" id="lecture_recommend_del_cancel">{{LECTURETICKET_QUNBSPNBSPXIAO}}</div>
		<div class="lecture_recommend_dialog_submit" id="lecture_recommend_del_confirm">{{LECTURETICKET_QUENBSPNBSPDING}}</div>
