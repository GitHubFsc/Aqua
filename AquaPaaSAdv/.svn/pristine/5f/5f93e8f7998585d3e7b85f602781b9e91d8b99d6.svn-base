<style>
    #adv_sys_user_manage_position_list{
        overflow: visible !important;
    }

    #adv_sys_user_manage_position_list table {
        overflow: visible !important;
    }

    #adv_sys_user_manage_position_list table > tbody > tr > td:last-child {
        overflow: visible !important;
    }
    #adv_sys_user_manage_position_list tr .opr {
        color: #3b95ba;
        margin: 0 8px;
        cursor: pointer;
        display: inline-block;
        position: relative;
        text-decoration: underline;
    }
    #adv_sys_user_manage_position_list tr .opr:last-child {
        margin: 0 0px;
    }
    #adv_sys_user_manage_position_list tr .opr img {
        vertical-align: middle;
    }
    #adv_sys_user_manage_position_list tr .opr_more {
        display: inline-block;
        background: inherit
    }
    #adv_sys_user_manage_position_list tr .opr_more .opr_more_detail {
        display: none;
        position: absolute;
        width: 73px;
        box-sizing: border-box;
        border: 1px solid #E2E2E2;
        z-index: 2;
        background: inherit;
        top: 12px;
        right: 22px;
        text-align: right;
    }
    #adv_sys_user_manage_position_list tr .opr_more .opr_more_detail .opr {
        height: 24px;
        line-height: 24px;
        box-sizing: border-box;
        border-bottom: 1px solid #E2E2E2;
        display: block;
        margin: 0px;
        padding-right: 10px;
        text-decoration: none;
    }
    #adv_sys_user_manage_position_list tr .opr_more .opr_more_detail .opr:last-child {
        border-bottom: 0px solid #E2E2E2;
    }
    #adv_sys_user_manage_position_list tr .opr_more.open .opr_more_detail {
        display: block;
    }
</style>
