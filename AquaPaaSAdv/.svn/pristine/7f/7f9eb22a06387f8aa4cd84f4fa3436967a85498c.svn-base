<div class="_dialog" id="add_site">
  <div class="_dialog_title _flex">
    <div class="title_content">{{SUCAI_ADD_SITE}}</div>
    <div class="_dialog_win_close popup_dialog_clear"></div>
  </div>
  <div class="_dialog_body">
    <div class="row">
      <div class="hint">{{SUCAI_SITE_TARGET}}</div>
      <div class="site_list" id="site_list"></div>
    </div>
    <div class="row">
      <div class="hint">{{SUCAI_SITE_DISPATCH}}</div>
      <input type="text" id="dispatch_times">
      <div class="hint hint_after">{{SUCAI_TIME}}</div>
        <div class="hint" style="flex: 1;"></div>
      <div class="hint">{{SUCAI_SITE_PLACEMENTTIMES}}</div>
      <input type="text" id="placement_times" disabled>
      <div class="hint hint_after">{{SUCAI_TIME}}</div>
    </div>
    <div class="group">
      <div class="row">
        <div class="hint">{{SUCAI_SITE_TEMINALCOUNT_SET}}</div>
        <div class="radio1 teminalcountset focus" data-value="1"></div>
        <div class="radio1_hint1" style="padding-left: 0 10px;margin: 0;">{{SUCAI_EDITLIMIT_YES}}</div>
        <div class="radio1 teminalcountset" data-value="0"></div>
        <div class="radio1_hint2" style="padding-left: 0 10px;margin: 0;">{{SUCAI_EDITLIMIT_NO}}</div>
          <div class="hint" style="flex: 1;"></div>
        <div class="hint">{{SUCAI_SITE_TEMINEL_COUNT}}</div>
        <input type="text" id="teminal_counts">
        <div class="hint hint_after">{{SUCAI_TIME}}</div>
      </div>
      <div class="row">
        <div class="hint">{{SUCAI_SITE_TEMINAL_PERIOD_SET}}</div>
        <div class="radio1 teminalperiodset focus" data-value="1"></div>
        <div class="radio1_hint1" style="padding-left: 0 10px;margin: 0;">{{SUCAI_EDITLIMIT_YES}}</div>
        <div class="radio1 teminalperiodset" data-value="0"></div>
        <div class="radio1_hint2" style="padding-left: 0 10px;margin: 0;">{{SUCAI_EDITLIMIT_NO}}</div>
          <div class="hint" style="flex: 1;"></div>
        <div class="hint">{{SUCAI_SITE_TEMINAL_PERIOD_TIME}}</div>
        <input type="text" id="tenimal_period">
        <div class="hint hint_after">{{SUCAI_PERIOD}}</div>
      </div>
      <div class="row">
        <div class="hint" style="flex: 1;"></div>
        <div class="hint" style="flex: 1;">{{SUCAI_SITE_TEMINAL_PERIOD_COUNT}}</div>
        <input type="text" id="tenimal_period_times">
        <div class="hint hint_after">{{SUCAI_TIME}}</div>
      </div>
    </div>
  </div>
  <div class="_dialog_foot">
    <div class="cancel _dialog_btn">{{UTILS_DIALOG_CANCEL}}</div>
    <div class="confirm _dialog_btn">{{UTILS_DIALOG_SUBMIT}}</div>
  </div>
</div>
