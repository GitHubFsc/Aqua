function getCurrentTime(){
	var name = ["日","一","二","三","四","五","六"];
	var d = new Date();
	var Y = d.getFullYear();
	var M = d.getMonth()+1;
	M = M<10?("0"+M):M;
	var D = d.getDate();
	D = D<10?("0"+D):D;
	var day = "星期"+name[parseInt(d.getDay())];
	//var now = "当前时间"+Y+"/"+(M+1)+"/"+D;
	var h = d.getHours();
	h = h<10?("0"+h):h;
	var m = d.getMinutes();
	m = m<10?("0"+m):m;
	var s = d.getSeconds();
	s = s<10?("0"+s):s;
	//var now = "当前时间:"+Y+"/"+M+"/"+D+" "+day+" "+h+":"+m+":"+s;
	var now = Y+"/"+M+"/"+D+" "+day+" "+h+":"+m+":"+s;
	var now1 = h+":"+m+":"+s;
	return now1;
	//alert(now);
	//document.getElementById("text0").value = now;
	//window.setTimeout("getCurrentTime();",1000);
}

