function grabEvent(event){
	var keyCode = event.which;
	//alert(keyCode)
	iPanel.debug("keyCode_vod_play="+keyCode);
	switch(keyCode){
		case 3:
		case 1029:
			if(playFlag){
				backwardPlay(2);
			}
			return 0;
			break;
		case 4:
		case 1028:
			if(playFlag){
				forwardPlay(2);
			}
			return 0;
			break;
		case 1024:
			if(playFlag){
				startus = media.AV.status;
				if(startus != "play"){
					media.AV.play();	
				}
				seekn = 0;
				showPF(1);
			}
			return 0;
			break;
		case 1026:
			if(playFlag){
				startus = media.AV.status;
				if(startus != "pause"){
					media.AV.pause();
					showAvStatus("pause");
				}
				seekn = 0;
				showPF(1);
			}
			return 0;
			break;
		case 13:
			if(playFlag){
				doSelect();
			}
			return 0;
			break;
		case 340:
		case 1025:
			//iPanel.back();
			history.back();
			//window.location.href = "../movie.htm";
			return 0;
			break;
		case 339:
			//iPanel.back();
			history.back();
			//window.location.href = "../movie.htm";
			return 0;
			break;
		case 512:
			window.location.href = "../index.htm";
			return 0;
			break;
		case 1://
		case 595://音量+
			iPanel.debug("_______volumetest___volup");
			changeVolume(1);
			return 0;
			break;
		case 2:
		case 596://音量-
			iPanel.debug("_______volumetest___voldown");
			changeVolume(-1);
			return 0;
			break;
		case 597://静音
			//alert("静音");
			iPanel.debug("_______volumetest___mute");
			mute_status = volume.actionMute();
			iPanel.debug("_______mute"+mute_status);
			iPanel.debug("_______ismute"+media.sound.isMute);
			return 0;
			break;
		case 598://声道
		case 802://功能键？
			iPanel.debug("_______volumetest___audiomode");
			show_track();
			iPanel.debug("_______checktrack"+media.sound.mode);
			return 0;
			break;
		case 5202://setup成功
			$("loading").style.visibility = "hidden";
			//play();
			seek();
			return 0;
			break;
		case 5203://setup失败
			flag_5203 = true;
			showOverMain("视频连接失败");
			return 0;
			break;
		case 5205://播放成功
			media.sound.value = 48;
			startus =  media.AV.status ;
			showAvStatus(startus);
			return 0;
			break;
		case 5206://播放失败
			//showOverMain("视频打开失败");
			return 0;
			break;
		case 5225:
			var p2 = event.modifiers;
			if(p2 == 500 || p2 == 503 || p2 == 404){
				var text = "播放失败 5225:"+p2;
				showOverMain(text);
			}
			//alert("5225:"+p2);
			return 0;
			break;
		case 5226:
			hiddenOverMain();
			return 0;
			break;
		case 5227:
			showOverMain("数据连接失败");
			return 0;
			break;
		case 5210://播放完毕
			showOverOk("节目播放完毕,3秒后退出");
			playFlag = false;
			setTimeout(function(){window.location.href = "../movie.htm";},3000);
			return 0;
			break;
	}
        return 0;
}

